<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
<TableSelectDialog
		noDataText="No Products Found"
		title="Select Product"
		search="handleSearch"
		confirm="handleClose"
		cancel="handleClose"
		items="{
			path : '/ProductCollection',
			sorter : {
				path : 'Name',
				descending : false
			}
		}">
		
		<columns>
			<Column width="12em">
				<header>
					<Text text="Operation Number" />
				</header>
			</Column>
			<Column width="12em">
				<header>
					<Text text="Operation Description" />
				</header>
			</Column>
			<Column width="12em">
				<header>
					<Text text="Labour Hours" />
				</header>
			</Column>
		</columns>
		<ColumnListItem>
			<cells>
				<Text text="{SupplierName}" />
				<Text text="{SupplierName}" />
				<Text text="{SupplierName}" />
			</cells>
		</ColumnListItem>
	</TableSelectDialog>
</core:FragmentDefinition>