<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:u="sap.ui.unified" xmlns:l="sap.ui.layout"
	xmlns="sap.m" xmlns:table="sap.ui.table" xmlns:custom="zclaimProcessing.control">
	<VBox class="sapUiSmallMargin clFormTitleGap"  id="idpart01Form" >
		<Panel width="auto" class="sapUiSmallMarginBottom" accessibleRole="Region">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="{i18n>ClaimDetails}"/>
					<ToolbarSpacer/>
					<Title text="{LocalDataModel>/WarrantyClaimNumber}" textAlign="End"/>
				</Toolbar>
			</headerToolbar>
			<f:SimpleForm editable="true" layout="ResponsiveGridLayout" title="" labelSpanXL="6" labelSpanL="6" labelSpanM="12" labelSpanS="12"
				emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="4" columnsL="4" columnsM="6">
				<f:content>
					<core:Title text=""/>
					<Label text="{i18n>ClaimType}"/>
					<ComboBox showSecondaryValues="true" required="true" items="{path:'LocalDataModel>/ClaimGroupSet'}" selectionChange="onSelectClaimTpe"
						selectedKey="{HeadSetData>/WarrantyClaimType}" enabled="{DateModel>/claimTypeEn}" valueState="{DateModel>/claimTypeState}"
						value="{path:'HeadSetData>/WarrantyClaimType', type : 'sap.ui.model.type.String', constraints : { minLength : 2, maxLength : 50 } }">
						<core:ListItem text="{LocalDataModel>ALMClaimType}" key="{LocalDataModel>ALMClaimType}" additionalText="{LocalDataModel>ALMClaimTypeDes}"/>
					</ComboBox>
					<Label text="{i18n>SubmissionClaimType}" visible="{DateModel>/warrantySubmissionClaim}"/>
					<ComboBox showSecondaryValues="true" required="true" visible="{DateModel>/warrantySubmissionClaim}"
						items="{path:'LocalDataModel>/DataSubmissionClaim'}" selectionChange="onSelectClaimTpe" selectedKey="{HeadSetData>/WarrantyClaimSubType}"
						enabled="{DateModel>/claimTypeEn}" valueState="{DateModel>/claimTypeState}"
						value="{path:'HeadSetData>/WarrantyClaimSubType', type : 'sap.ui.model.type.String', constraints : { minLength : 2, maxLength : 50 } }">
						<core:ListItem text="{LocalDataModel>ALMClaimType}" key="{LocalDataModel>ALMClaimType}" additionalText="{LocalDataModel>ALMClaimTypeDes}"/>
					</ComboBox>
					<core:Title text=""/>
					<Label text="{i18n>FirstSubmissionDate}"/>
					<Text
						text="{ path: 'HeadSetData>/ReferenceDate', type: 'sap.ui.model.type.Date',  formatOptions: { style: 'medium', pattern: 'YYYY-MM-dd', strictParsing: true , UTC: true}}"/>
					<Label text="{i18n>ClaimAge}"/>
					<Text text="{HeadSetData>/ClaimAge}"/>
					<Label text="{i18n>LastModifiedby}"/>
					<Text text="{HeadSetData>/NameOfPersonRespWhoChangedObj}"/>
					<core:Title text=""/>
					<Label text="{i18n>LastSubmissionDate}"/>
					<Text
						text="{ path: 'HeadSetData>/DateOfApplication', type: 'sap.ui.model.type.Date',  formatOptions: { style: 'medium', pattern: 'YYYY-MM-dd', strictParsing: true , UTC: true}}"/>
					<Label text="{i18n>FinalProcessedDate}"/>
					<Text
						text="{ path: 'HeadSetData>/FinalProcdDate', type: 'sap.ui.model.type.Date',  formatOptions: { style: 'medium', pattern: 'YYYY-MM-dd', strictParsing: true , UTC: true} }"/>
					<core:Title text=""/>
					<Button text="{i18n>SaveClaim}" class="sapUiSmallMarginEnd" type="Emphasized" press="onSaveClaim" visible="{DateModel>/saveClaimSt}">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					<Button text="{i18n>Edit}" class="sapUiSmallMarginEnd" type="Emphasized" press="onEditClaim" visible="{DateModel>/claimEditSt}">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					<Button text="{i18n>SaveClaim}" class="sapUiSmallMarginEnd" enabled="{DateModel>/updateEnable}" type="Emphasized" press="onUpdateClaim"
						visible="{DateModel>/updateClaimSt}">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					<Button text="{LocalDataModel>/copyClaimAuthText}" enabled="{DateModel>/copyClaimEnable}" type="Default" press="onCopyClaim">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					
					<Button text="{i18n>Approve}" visible="{DateModel>/authAcClm}" type="Accept" press="onApproveClaim">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					
					<Button text="{i18n>Reject}" visible="{DateModel>/authRejClm}" type="Reject" press="onRejectClaim">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
				</f:content>
			</f:SimpleForm>
		</Panel>
		<f:SimpleForm id="idClaimMainForm" editable="{DateModel>/oFormEdit}" title="Repair Order Information" layout="ResponsiveGridLayout"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0"
			emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<Label text="{i18n>DealerName}"/>
				<Text text="{HeadSetData>/CustomerFullName}"/>
				<Label text="{i18n>DealerClaim}"/>
				<Input
					value="{path:'HeadSetData>/ExternalNumberOfClaim', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 40 } }"
					required="true" id="idDealerClaim" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="{i18n>RepairDate}"/>
				<!--<DatePicker class="clDatePicker" required="true" value="{path:'HeadSetData>/RepairDate',type : 'sap.ui.model.type.Date'}" editable="{DateModel>/oFormEdit}"/>-->
				<DatePicker class="clDatePicker"  id="id_Date" required="true" change="onChangeDate" value="{path:'HeadSetData>/RepairDate', type: 'sap.ui.model.type.Date',  
				formatOptions: { style: 'medium', pattern: 'YYYY-MM-dd', strictParsing: true , UTC: true}}" editable="{DateModel>/oFormEdit}"/>
				<Label text="{i18n>DealerContact}"/>
				<Input required="true"
					value="{path:'HeadSetData>/DealerContact', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 30 }}"
					id="idDealerContact" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="{i18n>RepairOrder}"/>
				<Input required="true"
					value="{path:'HeadSetData>/RepairOrderNumberExternal', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 10 }}"
					id="idRepairOrder" editable="{DateModel>/oFormEdit}"/>
				<Label text="{i18n>ROOdometer}"/>
				<custom:CustomInput
					value="{path:'HeadSetData>/Odometer', type : 'sap.ui.model.type.String', constraints : { minLength : 2, maxLength : 7 }}"
					required="{DateModel>/OdometerReq}" visible="{DateModel>/OdometerReq}" id="idOdometer" editable="{DateModel>/oFormEdit}"/>
				<Text text="{LocalDataModel>/MsrUnit}" visible="{DateModel>/OdometerReq}"/>
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm editable="" id="idVehicleInfo" title="{i18n>VehicleInformation} - {path:'LocalDataModel>/DataVinDetails/VehicleDescription'}"
			layout="ResponsiveGridLayout" labelSpanXL="6" labelSpanL="6" labelSpanM="6" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
			emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content >
				<core:Title text=""/>
				<RadioButtonGroup enabled="{DateModel>/DisableRadio}" selectedIndex="0" columns="2" id="idRequestType" select="onSelectRequestType" editable="{DateModel>/oFormEdit}">
					<buttons>
						<RadioButton groupName="E" text="{i18n>VIN}"/>
						<RadioButton groupName="E" text="{i18n>NonVehicle}"/>
					</buttons>
				</RadioButtonGroup>
				<Input required="{DateModel>/OdometerReq}"
					value="{path:'HeadSetData>/ExternalObjectNumber', type : 'sap.ui.model.type.String', constraints : { minLength : 16, maxLength : 17 }}"
					visible="{DateModel>/OdometerReq}" id="idVinNum" change="onEnterVIN" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="{i18n>RegDate}"/>
				<Text
					text="{path:'LocalDataModel>/DataVinDetails/RegDate', type: 'sap.ui.model.type.Date',  formatOptions: { style: 'medium', pattern: 'YYYY-MM-dd'}}"/>
				<Label text="{i18n>VehicleAge}"/>
				<Text text="{LocalDataModel>/DataVinDetails/VehicleDays}" id="idVehicleAge"/>
				<core:Title text=""/>
				<Label text="{i18n>ForeignVIN}" vAlign="Middle"/>
				<Text text="{LocalDataModel>/DataVinDetails/ForeignVIN}" id="idForeignVin"/>
				<Button icon="sap-icon://message-warning" type="Transparent" press="onPressForeignVin" class="clWarning"/>
				<Label text="{i18n>WrittenOff}" vAlign="Middle"/>
				<Text text="{LocalDataModel>/DataVinDetails/WrittenOff}" id="idWrittenoff"/>
				<Button icon="sap-icon://message-warning" type="Transparent" press="onPressWrittenOff" class="clWarning"/>
				<Label text="{i18n>SpecialVINReview}" vAlign="Middle"/>
				<Text text="{LocalDataModel>/DataVinDetails/SpecialVINReview}" id="idSpecialVin"/>
				<Button icon="sap-icon://message-warning" type="Transparent" press="onPressSpecialVin" class="clWarning"/>
			</f:content>
		</f:SimpleForm>
		<VBox visible="{DateModel>/ShipmentVisible}">
			<f:SimpleForm editable="true" id="idShipmentInfo" title="{i18n>ShipmentInformation}" layout="ResponsiveGridLayout" labelSpanXL="12"
				labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0"
				columnsXL="4" columnsL="4" columnsM="3" singleContainerFullSize="false">
				<f:content>
					<core:Title text=""/>
					<Label text="{i18n>DeliveringCarrier}"/>
					<ComboBox required="true" editable="{DateModel>/oFormEdit}" items="" selectedKey="" value="{HeadSetData>/DeliveringCarrier}">
						<core:ListItem key="" text="Hansen's"/>
						<core:ListItem key="" text="United Road"/>
						<core:ListItem key="" text="MCGill"/>
						<core:ListItem key="" text="Harvey"/>
						<core:ListItem key="" text="Laberge"/>
						<core:ListItem key="" text="Others"/>
					</ComboBox>
					<core:Title text=""/>
					<Label text="{i18n>Probill}"/>
					<Input value="{HeadSetData>/ProbillNum}" required="true"/>
					<core:Title text=""/>
					<Label text="{i18n>DeliveryDate}"/>
					<DatePicker class="clDatePicker" required="true" value="{path:'HeadSetData>/DeliveryDate', type: 'sap.ui.model.type.Date',  formatOptions: { style: 'medium', pattern: 'YYYY-MM-dd', strictParsing: true , UTC: true}}" editable="{DateModel>/oFormEdit}"/>
					<core:Title text=""/>
					<Label text="{i18n>Delivery}"/>
					<ComboBox required="true" editable="{DateModel>/oFormEdit}" items="" selectedKey="" value="{HeadSetData>/Delivery}">
						<core:ListItem key="" text="{i18n>Attended}"/>
						<core:ListItem key="" text="{i18n>Unattended}"/>
					</ComboBox>
				</f:content>
			</f:SimpleForm>
			<HBox justifyContent="End">
				<Button text="{i18n>AddDamageLine}" press="onAddDamageLine" type="Emphasized"  enabled="{DateModel>/oDamageLineBtn}"/>
				<Button text="{i18n>UpdateDamageLine}" press="onUpdateDamageLine" class="sapUiTinyMarginBeginEnd" enabled="{DateModel>/oDamageLineBtn}"/>
				<Button text="{i18n>DeleteDamageLine}" press="onDeleteDamageLine" class="sapUiTinyMarginBeginEnd" type="Reject"
					enabled="{DateModel>/oDamageLineBtn}"/>
			</HBox>
			<VBox id="idDamageArea" class="clBgColor sapUiSmallMarginTop" wrap="Wrap" width="100%">
				<f:SimpleForm editable="true" labelSpanL="4" width="100%" title="" labelSpanXL="12" layout="ResponsiveGridLayout" emptySpanXL="0"
					emptySpanL="0" columnsM="3" columnsL="4" columnsXL="4">
					<f:content>
						<core:Title text=""/>
						<Label text="{i18n>Area}"/>
						<ComboBox showSecondaryValues="true" editable="{DateModel>/oFormEdit}" required="true" filterSecondaryValues="true" items="{LocalDataModel>/DataAreaCode}" selectedKey=""
							value="{HeadSetData>/DmgAreaCode}" enabled="{DateModel>/damageLine}">
							<core:ListItem key="{LocalDataModel>DmgAreaCode}" text="{LocalDataModel>DmgAreaCode}" additionalText="{LocalDataModel>DmgAreaCodeDesc}"/>
						</ComboBox>
						<core:Title text=""/>
						<Label text="{i18n>Type}"/>
						<ComboBox showSecondaryValues="true" editable="{DateModel>/oFormEdit}" required="true" selectedKey="" value="{HeadSetData>/DmgTypeCode}" filterSecondaryValues="true"
							items="{LocalDataModel>/DataTypeCode}" enabled="{DateModel>/damageLine}">
							<core:ListItem key="{LocalDataModel>DmgTypeCode}" text="{LocalDataModel>DmgTypeCode}" additionalText="{LocalDataModel>DmgTypeCodeDesc}"/>
						</ComboBox>
						<core:Title text=""/>
						<Label text="{i18n>Severity}"/>
						<ComboBox showSecondaryValues="true" editable="{DateModel>/oFormEdit}" required="true" selectedKey="" value="{HeadSetData>/DmgSevrCode}" filterSecondaryValues="true"
							items="{LocalDataModel>/DataSeverety}" enabled="{DateModel>/damageLine}">
							<core:ListItem key="{LocalDataModel>DmgSevrCode}" text="{LocalDataModel>DmgSevrCode}" additionalText="{LocalDataModel>DmgSevrCodeDesc}"/>
						</ComboBox>
						<core:Title text=""/>
						<Label text=" "/>
						<Button text="{i18n>SaveDamage}" press="onSaveDamage"  type="Emphasized" enabled="{DateModel>/damageLine}"/>
						<Link text="{i18n>CancelClaim}" enabled="{DateModel>/damageLine}"/>
					</f:content>
				</f:SimpleForm>
			</VBox>
			<Table id="idDamageDetailTable" mode="SingleSelectLeft" items="{LocalDataModel>/DataItemDamageSet}" class="sapUiMediumMarginTop"
				selectionChange="onSelectDamageItem">
				<columns>
					<Column>
						<Text text="{i18n>Line}"/>
					</Column>
					<Column>
						<Text text="{i18n>AreaCode}"/>
					</Column>
					<Column>
						<Text text="{i18n>AreaDescription}"/>
					</Column>
					<Column>
						<Text text="{i18n>TypeCode}"/>
					</Column>
					<Column>
						<Text text="{i18n>TypeDescription}"/>
					</Column>
					<Column>
						<Text text="{i18n>SeverityCode}"/>
					</Column>
					<Column>
						<Text text="{i18n>SeverityDescription}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<Text text="{LocalDataModel>LineNum}"/>
							<Text text="{LocalDataModel>DmgAreaCode}"/>
							<Text text="{LocalDataModel>DmgAreaCodeDesc}"/>
							<Text text="{LocalDataModel>DmgTypeCode}"/>
							<Text text="{LocalDataModel>DmgTypeCodeDesc}"/>
							<Text text="{LocalDataModel>DmgSevrCode}"/>
							<Text text="{LocalDataModel>DmgSevrCodeDesc}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</VBox>
		<Table id="idECPAGR" headerText="{i18n>ActiveAgreeement}" mode="SingleSelectLeft" items="{path:'LocalDataModel>/AgreementDataECP'}"
			class="sapUiMediumMarginTop" selectionChange="onSelectECP" visible="{DateModel>/oECPfields}">
			<columns>
				<Column>
					<Label text="{i18n>ECPAgreement}" textAlign="Center" width="100%"/>
				</Column>
				<Column>
					<Label text="{i18n>PlanCode}" textAlign="Center" width="100%"/>
				</Column>
				<Column>
					<Label text="{i18n>Status}" textAlign="Center" width="100%"/>
				</Column>
				<Column>
					<Label text="{i18n>StatusDate}" textAlign="Center" width="100%"/>
				</Column>
				<Column>
					<Label text="{i18n>ExpiryDate}" textAlign="Center" width="100%"/>
				</Column>
				<Column>
					<Label text="{i18n>ExpiryKM}" textAlign="Center" width="100%"/>
				</Column>
				<Column></Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{LocalDataModel>AgreementNumber}" wrapping="true"/>
						<Text text="{LocalDataModel>PlanType}" wrapping="true"/>
						<Text text="{LocalDataModel>bcc_agr_st_cd}" wrapping="true"/>
						<Text
							text="{path:'LocalDataModel>StatusDate', type: 'sap.ui.model.type.Date', constraints: { displayFormat: 'Date' }, formatOptions: { style: 'medium', strictParsing: true , UTC: true}}"
							wrapping="true"/>
						<Text
							text="{path:'LocalDataModel>AgreementthruDate', type: 'sap.ui.model.type.Date',  formatOptions: { style: 'medium', pattern: 'YYYY-MM-dd'}}"
							wrapping="true"/>
						<Text text="{LocalDataModel>AgreementthruKMreading}" wrapping="true"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
		
		<f:SimpleForm editable="" title="" layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12"
			adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="5" columnsL="5" columnsM="3"
			singleContainerFullSize="false" visible="{DateModel>/oECPfields}">
			<f:content>
				<core:Title text=""/>
				<Label text="{i18n>ClientLastName}"/>
				<Input value="{HeadSetData>/CustomerFullName}" required="true"/>
				<core:Title text=""/>
				<Label text="{i18n>PostalCode}"/>
				<Input value="{HeadSetData>/CustomerPostalCode}" required="true"/>
				<core:Title text=""/>
				<Label text=" "/>
				<Button text="{i18n>LookupECPAgreements}"/>
				<core:Title text=""/>
				<Label text=" "/>
				<Button text="{i18n>PerformCIC}"/>
				<core:Title text=""/>
				<Label text="{i18n>AgreementNumber}"/>
				<Text text="{HeadSetData>/AgreementNumber}"/>
			</f:content>
		</f:SimpleForm>
		<VBox visible="{DateModel>/RepairdDetailVisible}">
			<f:SimpleForm visible="{DateModel>/RepairdDetailVisible}" editable="{DateModel>/oFormEdit}" title="{i18n>RepairDetails}"
				layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
				emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
				<f:content>
					<core:Title text=""/>
					<Label text="{i18n>OFP}"/>
					<Input value="{path:'HeadSetData>/OFP'}" editable="{DateModel>/oFormEdit}" enabled="{DateModel>/ofpEnabled}"/>
					<Label text="{i18n>MainOp}"/>
					<Input value="{path:'HeadSetData>/MainOpsCode'}" editable="{DateModel>/oFormEdit}"/>
					<Label text="{i18n>BatteryTestCode}"/>
					<Input value="{HeadSetData>/BatteryTestCode}" editable="{DateModel>/oFormEdit}" enabled="{DateModel>/oBatteryTestEnable}"/>
					<Label text="{i18n>Condition}"/>
					<TextArea required="true" editable="{DateModel>/oFormEdit}"
						value="{path:'HeadSetData>/ZCondition', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 100 }}"></TextArea>
					<core:Title text=""/>
					<Label text="{i18n>OFPDescription}" />
					<Text text="{path:'LocalDataModel>/OFPDescription'}" />
					<Label text="{i18n>MainOpDescription}"/>
					<Text text="{path:'LocalDataModel>/MainOpsCodeDescription'}"/>
					<Label text="{i18n>FieldActionCode}"/>
					<Input required="{path:'DateModel>/oFieldActionInput'}" value="{path:'HeadSetData>/FieldActionReference'}" editable="{DateModel>/oFormEdit}"
						enabled="{DateModel>/oFieldActionInput}" change="onFieldActionInput"/>
					<Label text="{i18n>Cause}"/>
					<TextArea required="true" editable="{DateModel>/oFormEdit}"
						value="{path:'HeadSetData>/Cause',type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 100 }}"></TextArea>
					<core:Title text=""/>
					<Label text="{i18n>AppliedCoverage}"/>
					<Text text="{HeadSetData>/AppliedWarrantyCoverage}"/>
					<Label text="{i18n>T1 Code}"/>
					<ComboBox  required="{DateModel>/enabledT1}" items="{ path: '/Zt1DescSrchhelpSet' }" selectedKey="{path:'HeadSetData>/T1WarrantyCodes', type : 'sap.ui.model.type.String', constraints : { minLength : 1, maxLength : 100 }}"
						enabled="{DateModel>/enabledT1}" editable="{DateModel>/oFormEdit}">
						<core:ListItem key="{T1}" text="{path:'T1'} - {T1Text}" />
					</ComboBox>
					<!--<Select selectedKey="{HeadSetData>/T1WarrantyCodes}" items="{ path: '/Zt1DescSrchhelpSet' }" enabled="{DateModel>/oFormEdit}">-->
					<!--	<core:Item key="{T1}" text="{T1} - {T1Text}"/>-->
					<!--</Select>-->
					<Label text="{i18n>T2Code}"/>
					<ComboBox required="{DateModel>/enabledT2}"  items="{ path: '/Zt2DescSrchhelpSet' }" selectedKey="{path:'HeadSetData>/T2WarrantyCodes', type : 'sap.ui.model.type.String', constraints : { minLength : 1, maxLength : 100 }}"
						enabled="{DateModel>/enabledT2}" editable="{DateModel>/oFormEdit}">
						<core:ListItem key="{path:'T2'}" text="{T2} - {T2Text}"/>
					</ComboBox>
					<!--<Select selectedKey="{HeadSetData>/T2WarrantyCodes}" items="{ path: '/Zt2DescSrchhelpSet' }" enabled="{DateModel>/oFormEdit}">-->
					<!--	<core:Item key="{T2}" text="{T2} - {T2Text}"/>-->
					<!--</Select>-->
					<Label text="{i18n>Remedy}"/>
					<TextArea required="true" editable="{DateModel>/oFormEdit}"
						value="{path:'HeadSetData>/Remedy', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 100 }}"></TextArea>
				</f:content>
			</f:SimpleForm>
			<Button text="{i18n>ViewCCRAbbreviations}" press="onPressAbbr"/>
		</VBox>
		<f:SimpleForm visible="{DateModel>/AcA1}" editable="{DateModel>/oFormEdit}" title="Accessory Installation/Purchase"
			layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
			emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<Label text="{i18n>InstallDate}"/>
				<DatePicker class="clDatePicker" required="true" value="{path:'HeadSetData>/AccessoryInstallDate', type: 'sap.ui.model.type.Date',  formatOptions: { style: 'medium', pattern: 'YYYY-MM-dd', strictParsing: true , UTC: true}}"
					editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="{i18n>InstallOdometer}"/>
				<Input value="{path:'HeadSetData>/AccessoryInstallOdometer'}" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm visible="{DateModel>/P1p2}" editable="{DateModel>/oFormEdit}" title="Original/Previous Installation"
			layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
			emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<Label text="{i18n>PreviousROInvoice}"/>
				<Input required="{DateModel>/oPrevInvNumReq}" id="idPreInvNum" value="{path:'HeadSetData>/PreviousROInvoice'}" editable="{DateModel>/oFormEdit}"/>
				
				<core:Title text=""/>
				<Label text="{i18n>ROInvoiceDate}"/>
				<DatePicker class="clDatePicker" id="idPrInvDate"  required="{DateModel>/oPrevInvDateReq}" value="{path:'HeadSetData>/PreviousROInvoiceDate', type: 'sap.ui.model.type.Date',  
				formatOptions: { pattern: 'YYYY-MM-dd', strictParsing: true , UTC: true}}"
					editable="{DateModel>/oFormEdit}"/>
					
				<core:Title text=""/>
				<Label text="{i18n>ROOdometer}"/>
				<custom:CustomInput visible="{DateModel>/PreroOdometerVisible}" value="{path:'HeadSetData>/PreviousROOdometer'}" editable="{DateModel>/oFormEdit}"/>
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm editable="{DateModel>/oFormEdit}" title="{i18n>ClaimComments}" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3"
			labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="1" emptySpanL="1" emptySpanM="1" emptySpanS="0" columnsXL="1"
			columnsL="1" columnsM="1" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<TextArea height="8rem" value="{HeadSetData>/HeadText}" editable="{DateModel>/commentEditable}" enabled="{DateModel>/oFormEdit}" >
					<layoutData>
						<l:GridData span="XL8 L8 M8 S12"/>
					</layoutData>
				</TextArea>
				<Button text="Add" press="onAddComment">
					<layoutData>
						<l:GridData span="XL2 L2 M3 S4"/>
					</layoutData>
				</Button>
			</f:content>
		</f:SimpleForm>
	</VBox>
	<VBox>
		<Panel width="auto" class="sapUiSmallMarginBottom" accessibleRole="Region">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Supporting Documents"/>
				</Toolbar>
			</headerToolbar>
			<content>
				<f:SimpleForm editable="{DateModel>/oFormEdit}" title="" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3"
					labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="0" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
					singleContainerFullSize="false">
					<f:content>
						<UploadCollection  uploadEnabled="{LocalDataModel>/UploadEnable}"  change="onUplaodChange" id="UploadSupportingDoc" uploadComplete="onUploadComplete" fileDeleted="onFileDeleted"
							beforeUploadStarts="onBeforeUpload" items="{ path: 'ClaimModel>/items', templateShareable: false }">
							<!--<toolbar>-->
							<!--	<OverflowToolbar>-->
							<!--		<Breadcrumbs id="breadcrumbsSupportingDoc" currentLocationText="{ClaimModel>/currentLocationText}" links="{ClaimModel>/history}">-->
							<!--			<Link press="onBreadcrumbPress" text="{ClaimModel>fileName}"/>-->
							<!--		</Breadcrumbs>-->
							<!--		<ToolbarSpacer/>-->
							<!--		<UploadCollectionToolbarPlaceholder/>-->
							<!--	</OverflowToolbar>-->
							<!--</toolbar>-->
							<items>
								<UploadCollectionItem   documentId="{ClaimModel>DOC_ID}" fileName="{ClaimModel>FileName}"
									mimeType="{ClaimModel>MIMETYPE}" thumbnailUrl="{ClaimModel>url}" url="{ClaimModel>URI}" enableEdit="{settings>/enableEdit}"
									enableDelete="{settings>/enableDelete}" visibleDelete="{settings>/visibleDelete}" visibleEdit="{settings>/visibleEdit}"></UploadCollectionItem>
								<!--<UploadCollectionItem documentId="{LocalDataModel>DOC_ID}" fileName="{LocalDataModel>FileName}" mimeType="{LocalDataModel>MIMETYPE}"-->
								<!--	thumbnailUrl="{LocalDataModel>url}" url="{LocalDataModel>URI}" enableEdit="{settings>/enableEdit}" enableDelete="{settings>/enableDelete}"-->
								<!--	visibleDelete="{settings>/visibleDelete}" visibleEdit="{settings>/visibleEdit}">-->
							</items>
						</UploadCollection>
					</f:content>
				</f:SimpleForm>
			</content>
		</Panel>
		<Panel width="auto" class="sapUiSmallMarginBottom" accessibleRole="Region">
			<content>
				<table:Table class="classRCPTable" selectionMode="Single" rows="{path:'LocalDataModel>/ClaimSum'}" visibleRowCount="5"
			rowSelectionChange="onNavigate" selectionBehavior="Row" showNoData="true" threshold="5" enableBusyIndicator="true">
			<table:toolbar>
				<Toolbar>
					<Title text="{i18n>ClaimAmountSummary}" level="H2"/>
					<ToolbarSpacer/>
					<!--<Button tooltip="View Settings" icon="sap-icon://drop-down-list" press="handleViewSettingsDialogButtonPressed"/>-->
				</Toolbar>
			</table:toolbar>
			<table:columns autoResizable="true">
				<table:Column width="17rem">
					<table:multiLabels>
						<Label text="{i18n>Totalsincludedmarkupdiscounts}" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{path:'LocalDataModel>ItemType', formatter: 'zclaimProcessing.utils.formatter.fnFormatText'}" wrapping="false"/>
					</table:template>
				</table:Column>
				<table:Column width="11rem">
					<table:multiLabels>
						<Label text="{i18n>DealerClaimedAmount}" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{LocalDataModel>DealerClaim}" wrapping="false"/>
					</table:template>
				</table:Column>
				<table:Column width="11rem">
					<table:multiLabels>
						<Label text="{i18n>TCIApprovedAmount}" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{LocalDataModel>TCIAppr}" wrapping="false"/>
					</table:template>
				</table:Column>
				<table:Column width="11rem">
					<table:multiLabels>
						<Label text="{i18n>DifferenceCAD}" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{LocalDataModel>Diff}" wrapping="false"/>
					</table:template>
				</table:Column>
				<table:Column width="8rem" headerSpan="{multiHeaderConfig>/multiheader2}">
					<table:multiLabels>
						<Label text="{i18n>GSTHST}" textAlign="Center" width="100%"/>
						<Label text="{i18n>Amount}" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{LocalDataModel>GSTHSTAmt}" wrapping="false"/>
					</table:template>
				</table:Column>
				<table:Column width="8rem">
					<table:multiLabels>
						<Label text="{i18n>GSTHST}"/>
						<Label text="{i18n>Tax}" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{LocalDataModel>GSTHSTRate}" wrapping="false"/>
					</table:template>
				</table:Column>
				<table:Column width="8rem" headerSpan="{multiHeaderConfig>/multiheader2}">
					<table:multiLabels>
						<Label text="{i18n>PSTQST}" textAlign="Center" width="100%"/>
						<Label text="Amount" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{LocalDataModel>PSTQSTAmt}" wrapping="false"/>
					</table:template>
				</table:Column>
				<table:Column width="8rem">
					<table:multiLabels>
						<Label text="{i18n>PSTQST}"/>
						<Label text="{i18n>Tax}" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{LocalDataModel>PSTQSTRate}" wrapping="false"/>
					</table:template>
				</table:Column>
				<table:Column width="17rem">
					<table:multiLabels>
						<Label text="{i18n>TCIApprovedAmountIncludingTax}" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{LocalDataModel>TCIApprInclTax}" wrapping="false"/>
					</table:template>
				</table:Column>
			</table:columns>
		</table:Table>
			</content>
			<HBox alignContent="Center" justifyContent="Center" class="sapUiTinyMarginTop">
				<Button class="sapUiTinyMarginBeginEnd" press="onPressBack" text="{i18n>BackList}" type="Back"/>
				<Button class="sapUiTinyMarginBeginEnd" press="onCancelClaim" enabled="{LocalDataModel>/CancelEnable}" text="{i18n>CancelClaim}"
					type="Reject"/>
				<Button class="sapUiTinyMarginBeginEnd" text="{i18n>Print}"/>
				<Button class="sapUiTinyMarginBeginEnd" press="onStep01Next" enabled="{LocalDataModel>/step01Next}" text="{i18n>Next}" type="Emphasized"/>
			</HBox>
		</Panel>
	</VBox>
</core:FragmentDefinition>