<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:u="sap.ui.unified" xmlns:l="sap.ui.layout"
	xmlns="sap.m" xmlns:table="sap.ui.table" xmlns:custom="zclaimProcessing.control">
	<VBox class="sapUiSmallMargin clFormTitleGap" id="idpart01Form">
		<Panel width="auto" class="sapUiSmallMarginBottom" accessibleRole="Region">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="{i18n>ClaimDetails}"/>
					<ToolbarSpacer/>
					<Title text="{LocalDataModel>/WarrantyClaimNumber}" textAlign="End"/>
				</Toolbar>
			</headerToolbar>
			<f:SimpleForm editable="true" layout="ResponsiveGridLayout" title="" labelSpanXL="6" labelSpanL="6" labelSpanM="12" labelSpanS="12"
				emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="4" columnsL="4" columnsM="6">
				<f:content>
					<core:Title text=""/>
					<Label text="{i18n>ClaimType}"/>
					<custom:CustomCombo showSecondaryValues="true" id="idClaimType" required="true" items="{path:'LocalDataModel>/ClaimGroupSet'}"
						selectionChange="onSelectClaimTpe" selectedKey="{HeadSetData>/WarrantyClaimType}" enabled="{DateModel>/claimTypeEn}"
						valueState="{DateModel>/claimTypeState}"
						value="{path:'HeadSetData>/WarrantyClaimType', type : 'sap.ui.model.type.String', constraints : { minLength : 2, maxLength : 50 } }">
						<core:ListItem text="{LocalDataModel>TMCClaimType}" key="{LocalDataModel>TMCClaimType}" additionalText="{LocalDataModel>ALMClaimTypeDes}"/>
					</custom:CustomCombo>
					<Label text="{i18n>SubmissionClaimType}" visible="{DateModel>/warrantySubmissionClaim}"/>
					<custom:CustomCombo showSecondaryValues="true" required="true" visible="{DateModel>/warrantySubmissionClaim}"
						items="{path:'LocalDataModel>/DataSubmissionClaim'}" selectionChange="onSelectClaimTpe" selectedKey="{HeadSetData>/WarrantyClaimSubType}"
						enabled="{DateModel>/claimTypeEn}" valueState="{DateModel>/claimTypeState}"
						value="{path:'HeadSetData>/WarrantyClaimSubType', type : 'sap.ui.model.type.String', constraints : { minLength : 2, maxLength : 50 } }">
						<core:ListItem text="{LocalDataModel>TMCClaimType}" key="{LocalDataModel>TMCClaimType}" additionalText="{LocalDataModel>ALMClaimTypeDes}"/>
					</custom:CustomCombo>
					<core:Title text=""/>
					<Label text="{i18n>FirstSubmissionDate}"/>
					<Text text="{ path: 'HeadSetData>/ReferenceDate', type: 'sap.ui.model.type.Date', formatOptions: { style: 'medium', pattern: 'YYYY-MM-dd'}}"/>
					<Label text="{i18n>ClaimAge}"/>
					<Text text="{HeadSetData>/ClaimAge}"/>
					<Label text="{i18n>LastModifiedby}"/>
					<Text text="{LocalDataModel>/LoginId}"/>
					<core:Title text=""/>
					<Label text="{i18n>LastSubmissionDate}"/>
					<Text text="{ path: 'HeadSetData>/DateOfApplication', formatter:'zclaimProcessing.utils.formatter.fnDateFormat'}"/>
					<Label text="{i18n>FinalProcessedDate}"/>
					<Text text="{ path: 'HeadSetData>/FinalProcdDate', formatter:'zclaimProcessing.utils.formatter.fnDateFormat' }"/>
					<!--<Label text="{i18n>Status}"/>-->
					<!--<Text text="{HeadSetData>/ProcessingStatusOfWarrantyClm}"/>-->
					<core:Title text=""/>
					<Button text="{LocalDataModel>/SaveAuthClaim}" class="sapUiSmallMarginEnd" type="Emphasized" press="onSaveClaim"
						visible="{DateModel>/saveClaimSt}">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					<Button text="{i18n>Edit}" class="sapUiSmallMarginEnd" type="Emphasized" press="onEditClaim" visible="{DateModel>/claimEditSt}">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					<Button text="{LocalDataModel>/SaveAuthClaim}" id="idUpdateBtn" class="sapUiSmallMarginEnd" enabled="{DateModel>/updateEnable}"
						type="Emphasized" press="onUpdateClaim" visible="{DateModel>/updateClaimSt}">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					<Button text="{LocalDataModel>/copyClaimAuthText}" visible="{DateModel>/authHide}" enabled="{DateModel>/copyClaimEnable}" type="Default"
						press="onCopyClaim">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					<Button text="{i18n>Approve}" visible="{DateModel>/authAcClm}" type="Accept" press="onApproveClaim">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					<Button text="{i18n>Reject}" visible="{DateModel>/authRejClm}" type="Reject" press="onRejectClaim">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
				</f:content>
			</f:SimpleForm>
		</Panel>
		<f:SimpleForm id="idClaimMainForm" editable="{DateModel>/oFormEdit}" title="{i18n>RepairOrderInformation}" layout="ResponsiveGridLayout"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0"
			emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<Label text="{i18n>DealerName}"/>
				<Text text="{LocalDataModel>/BPOrgName}"/>
				<Label text="{i18n>DealerClaim}"/>
				<Input maxLength="40"
					value="{path:'HeadSetData>/ExternalNumberOfClaim', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 40 } }"
					required="true" id="idDealerClaim" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="{i18n>RepairDate}"/>
				<!--<DatePicker class="clDatePicker" required="true" value="{path:'HeadSetData>/RepairDate',type : 'sap.ui.model.type.Date'}" editable="{DateModel>/oFormEdit}"/>-->
				<DatePicker class="clDatePicker" id="id_Date" required="true" change="onChangeDate"
					value="{path:'HeadSetData>/RepairDate', type: 'sap.ui.model.type.Date', formatOptions: {pattern: 'yyyy-MM-dd'}}"
					editable="{DateModel>/oFormEdit}" visible="{DateModel>/oVisibleRepDate}"/>
				<!--constraints: { displayFormat: 'Date' }-->
				<Label text="{i18n>DealerContact}"/>
				<Input required="{DateModel>/oDealerContactReq}" maxLength="30" value="{path:'HeadSetData>/DealerContact'}" id="idDealerContact"
					editable="{DateModel>/oFormEdit}" enabled="{DateModel>/oDealerContactReq}"/>
				<core:Title text=""/>
				<Label text="{i18n>RepairOrder}"/>
				<Input required="true" maxLength="10"
					value="{path:'HeadSetData>/RepairOrderNumberExternal', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 10 }}"
					id="idRepairOrder" editable="{DateModel>/oFormEdit}" visible="{DateModel>/oVisibleReOrder}"/>
				<Label text="{i18n>ROOdometer}"/>
				<custom:CustomInput maxLength="6" enabled="{DateModel>/oOdoEnabled}"
					value="{path:'HeadSetData>/Odometer', type : 'sap.ui.model.type.String', constraints : { minLength : 1, maxLength : 6 }}"
					required="{DateModel>/OdometerReqMan}" visible="{DateModel>/OdometerReqMan}" id="idOdometer" editable="{DateModel>/oFormEdit}"/>
				<Text text="{LocalDataModel>/MsrUnit}" visible="{DateModel>/OdometerReqMan}"/>
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm editable="" id="idVehicleInfo" title="{i18n>VehicleInformation} - {path:'LocalDataModel>/DataVinDetails/VehicleDescription'}"
			layout="ResponsiveGridLayout" labelSpanXL="6" labelSpanL="6" labelSpanM="6" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
			emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content >
				<core:Title text=""/>
				<RadioButtonGroup enabled="{DateModel>/DisableRadio}" selectedIndex="{DateModel>/oRadioVinIndex}" columns="2" id="idRequestType"
					select="onSelectRequestType" editable="{DateModel>/oFormEdit}">
					<buttons>
						<RadioButton groupName="E" text="{i18n>VIN}"/>
						<RadioButton visible="{DateModel>/nonVinHide}" groupName="E" text="{i18n>NonVehicle}"/>
					</buttons>
				</RadioButtonGroup>
				<Input required="{DateModel>/OdometerReq}" maxLength="17"
					value="{path:'HeadSetData>/ExternalObjectNumber', type : 'sap.ui.model.type.String', constraints : { minLength : 16, maxLength : 17 }}"
					visible="{DateModel>/OdometerReq}" enabled="{DateModel>/OdometerReq}" id="idVinNum" change="onEnterVIN" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="{i18n>RegDate}"/>
				<Text text="{path:'LocalDataModel>/DataVinDetails/RegDate', formatter:'zclaimProcessing.utils.formatter.fnDateFormat'}"/>
				<Label text="{i18n>VehicleAge}"/>
				<Text text="{LocalDataModel>/VehicleMonths} {i18n>Months}" id="idVehicleAge"/>
				<core:Title text=""/>
				<Label text="{i18n>ForeignVIN}" vAlign="Middle"/>
				<Text text="{LocalDataModel>/DataVinDetails/ForeignVIN}" id="idForeignVin">
					<layoutData>
						<l:GridData span="XL1 L2 M3 S4"/>
					</layoutData>
				</Text>
				<core:Icon src="sap-icon://message-warning" class="clWarning" color="#f00" visible="{DateModel>/foreignVinInd}" press="onPressForeignVin">
					<core:layoutData>
						<FlexItemData/>
					</core:layoutData>
				</core:Icon>
				<!--<Button icon="sap-icon://message-warning" visible="{DateModel>/foreignVinInd}" type="Transparent" press="onPressForeignVin" class="clWarning"/>-->
				<Label text="{i18n>WrittenOff}" vAlign="Middle"/>
				<Text text="{LocalDataModel>/DataVinDetails/WrittenOff}" id="idWrittenoff">
					<layoutData>
						<l:GridData span="XL1 L2 M3 S4"/>
					</layoutData>
				</Text>
				<core:Icon src="sap-icon://message-warning" class="clWarning" color="#f00" visible="{DateModel>/writtenOffInd}" press="onPressWrittenOff">
					<core:layoutData>
						<FlexItemData/>
					</core:layoutData>
				</core:Icon>
				<!--<Button icon="sap-icon://message-warning" type="Transparent"  visible="{DateModel>/writtenOffInd}" press="onPressWrittenOff" class="clWarning"/>-->
				<Label text="{i18n>SpecialVINReview}" vAlign="Middle"/>
				<Text text="{LocalDataModel>/DataVinDetails/SpecialVINReview}" id="idSpecialVin">
					<layoutData>
						<l:GridData span="XL1 L2 M3 S4"/>
					</layoutData>
				</Text>
				<core:Icon class="clWarning" src="sap-icon://message-warning" color="#f00" visible="{DateModel>/specialVinInd}" press="onPressSpecialVin"></core:Icon>
				<!--<Button icon="sap-icon://message-warning" type="Transparent" visible="{DateModel>/specialVinInd}" press="onPressSpecialVin" class="clWarning"/>-->
			</f:content>
		</f:SimpleForm>
		<VBox visible="{DateModel>/ShipmentVisible}">
			<f:SimpleForm editable="true" id="idShipmentInfo" class="clShipmentInfo" title="{i18n>ShipmentInformation}" layout="ResponsiveGridLayout"
				labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0"
				emptySpanS="0" columnsXL="4" columnsL="4" columnsM="3" singleContainerFullSize="false">
				<f:content>
					<core:Title text=""/>
					<Label text="{i18n>DeliveringCarrier}"/>
					<custom:CustomCombo id="iDdelivCarrier" required="true" editable="{DateModel>/oFormEdit}" selectedKey=""
						value="{HeadSetData>/DeliveringCarrier}">
						<core:ListItem key="" text="Hansen's"/>
						<core:ListItem key="" text="United Road"/>
						<core:ListItem key="" text="MCGill"/>
						<core:ListItem key="" text="Harvey"/>
						<core:ListItem key="" text="Laberge"/>
						<core:ListItem key="" text="Others"/>
					</custom:CustomCombo>
					<core:Title text=""/>
					<Label text="{i18n>Probill}"/>
					<Input id="idProbill" value="{HeadSetData>/ProbillNum}" required="true"/>
					<core:Title text=""/>
					<Label text="{i18n>DeliveryDate}"/>
					<DatePicker class="clDatePicker" required="true"
						value="{path:'HeadSetData>/DeliveryDate', formatter:'zclaimProcessing.utils.formatter.fnDateFormat'}" editable="{DateModel>/oFormEdit}"/>
					<core:Title text=""/>
					<Label text="{i18n>Delivery}"/>
					<custom:CustomCombo id="idDelivery" required="true" editable="{DateModel>/oFormEdit}" items="" selectedKey=""
						value="{HeadSetData>/Delivery}">
						<core:ListItem key="" text="{i18n>Attended}"/>
						<core:ListItem key="" text="{i18n>Unattended}"/>
					</custom:CustomCombo>
				</f:content>
			</f:SimpleForm>
			<HBox justifyContent="End">
				<Button text="{i18n>AddDamageLine}" press="onAddDamageLine" type="Emphasized" enabled="{DateModel>/oDamageLineBtn}"/>
				<Button text="{i18n>UpdateDamageLine}" press="onUpdateDamageLine" class="sapUiTinyMarginBeginEnd" enabled="{DateModel>/oDamageLineBtn}"/>
				<Button text="{i18n>DeleteDamageLine}" press="onDeleteDamageLine" class="sapUiTinyMarginBeginEnd" type="Reject"
					enabled="{DateModel>/oDamageLineBtn}"/>
			</HBox>
			<VBox id="idDamageArea" class="clBgColor sapUiSmallMarginTop clShipmentInfo" wrap="Wrap" width="100%">
				<f:SimpleForm editable="true" labelSpanL="4" width="100%" title="" labelSpanXL="12" layout="ResponsiveGridLayout" emptySpanXL="0"
					emptySpanL="0" columnsM="3" columnsL="4" columnsXL="4">
					<f:content>
						<core:Title text=""/>
						<Label text="{i18n>Area}"/>
						<custom:CustomCombo showSecondaryValues="true" editable="{DateModel>/oFormEdit}" required="true" filterSecondaryValues="true"
							items="{LocalDataModel>/DataAreaCode}" selectedKey="" value="{HeadSetData>/DmgAreaCode}" enabled="{DateModel>/damageLine}">
							<core:ListItem key="{LocalDataModel>DmgAreaCode}" text="{LocalDataModel>DmgAreaCode}" additionalText="{LocalDataModel>DmgAreaCodeDesc}"/>
						</custom:CustomCombo>
						<core:Title text=""/>
						<Label text="{i18n>Type}"/>
						<custom:CustomCombo showSecondaryValues="true" editable="{DateModel>/oFormEdit}" required="true" selectedKey=""
							value="{HeadSetData>/DmgTypeCode}" filterSecondaryValues="true" items="{LocalDataModel>/DataTypeCode}" enabled="{DateModel>/damageLine}">
							<core:ListItem key="{LocalDataModel>DmgTypeCode}" text="{LocalDataModel>DmgTypeCode}" additionalText="{LocalDataModel>DmgTypeCodeDesc}"/>
						</custom:CustomCombo>
						<core:Title text=""/>
						<Label text="{i18n>Severity}"/>
						<custom:CustomCombo showSecondaryValues="true" editable="{DateModel>/oFormEdit}" required="true" selectedKey=""
							value="{HeadSetData>/DmgSevrCode}" filterSecondaryValues="true" items="{LocalDataModel>/DataSeverety}" enabled="{DateModel>/damageLine}">
							<core:ListItem key="{LocalDataModel>DmgSevrCode}" text="{LocalDataModel>DmgSevrCode}" additionalText="{LocalDataModel>DmgSevrCodeDesc}"/>
						</custom:CustomCombo>
						<core:Title text=""/>
						<Label text=" "/>
						<Button text="{i18n>SaveDamage}" press="onSaveDamage" type="Emphasized" enabled="{DateModel>/damageLine}"/>
						<Link text="{i18n>CancelClaim}" enabled="{DateModel>/damageLine}"/>
					</f:content>
				</f:SimpleForm>
			</VBox>
			<Table id="idDamageDetailTable" mode="SingleSelectLeft" items="{LocalDataModel>/DataItemDamageSet}" class="sapUiMediumMarginTop"
				selectionChange="onSelectDamageItem">
				<columns>
					<Column>
						<Text text="{i18n>Line}"/>
					</Column>
					<Column>
						<Text text="{i18n>AreaCode}"/>
					</Column>
					<Column>
						<Text text="{i18n>AreaDescription}"/>
					</Column>
					<Column>
						<Text text="{i18n>TypeCode}"/>
					</Column>
					<Column>
						<Text text="{i18n>TypeDescription}"/>
					</Column>
					<Column>
						<Text text="{i18n>SeverityCode}"/>
					</Column>
					<Column>
						<Text text="{i18n>SeverityDescription}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<Text text="{LocalDataModel>LineNum}"/>
							<Text text="{LocalDataModel>DmgAreaCode}"/>
							<Text text="{LocalDataModel>DmgAreaCodeDesc}"/>
							<Text text="{LocalDataModel>DmgTypeCode}"/>
							<Text text="{LocalDataModel>DmgTypeCodeDesc}"/>
							<Text text="{LocalDataModel>DmgSevrCode}"/>
							<Text text="{LocalDataModel>DmgSevrCodeDesc}"/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</VBox>
		<Table id="idECPAGR" headerText="{i18n>ActiveAgreeement}" mode="SingleSelectLeft" items="{path:'LocalDataModel>/AgreementDataECP'}"
			class="sapUiMediumMarginTop" selectionChange="onSelectECP" visible="{DateModel>/oECPfields}">
			<columns>
				<Column hAlign="Center">
					<Label text="{i18n>ECPAgreement}"/>
				</Column >
				<Column hAlign="Center">
					<Label text="{i18n>PlanCode}"/>
				</Column>
				<Column hAlign="Center">
					<Label text="{i18n>Status}"/>
				</Column>
				<Column hAlign="Center">
					<Label text="{i18n>StatusDate}"/>
				</Column>
				<Column hAlign="Center">
					<Label text="{i18n>ExpiryDate}"/>
				</Column>
				<Column hAlign="Center">
					<Label text="{i18n>ExpiryKM}"/>
				</Column>
				<Column hAlign="Center"></Column>
			</columns>
			<items>
				<ColumnListItem vAlign="Middle">
					<cells>
						<Link text="{LocalDataModel>AgreementNumber}" press="onPressAgreement"/>
						<Text text="{LocalDataModel>PlanType}" wrapping="true"/>
						<Text text="{LocalDataModel>AgreementStatus}" wrapping="true"/>
						<Text text="{path:'LocalDataModel>StatusDate', type: 'sap.ui.model.type.Date', formatOptions: { style: 'medium', pattern: 'YYYY-MM-dd'}}"
							wrapping="true"/>
						<Text
							text="{path:'LocalDataModel>AgreementthruDate', type: 'sap.ui.model.type.Date', formatOptions: { style: 'medium', pattern: 'YYYY-MM-dd'}}"
							wrapping="true"/>
						<Text text="{LocalDataModel>AgreementthruKMreading}" wrapping="true"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
		<f:SimpleForm editable="" title="" layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12"
			adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="4" columnsL="4" columnsM="3"
			singleContainerFullSize="false" visible="{DateModel>/oECPfields}">
			<f:content>
				<core:Title text=""/>
				<Label text="{i18n>ClientLastName}"/>
				<Input id="idClientLastName"
					value="{path:'HeadSetData>/CustomerFullName', type : 'sap.ui.model.type.String', constraints : { minLength : 2, maxLength : 40 }}"
					required="true"/>
				<core:Title text=""/>
				<Label text="{i18n>PostalCode}"/>
				<Input id="idPostalCode"
					value="{path:'HeadSetData>/CustomerPostalCode', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 10 }}"
					required="true"/>
				<!--<core:Title text=""/>-->
				<!--<Label text=" "/>-->
				<!--<Button press="onPressLookUpECP" text="{i18n>LookupECPAgreements}"/>-->
				<core:Title text=""/>
				<Label text=" " class="clTextIndent"/>
				<!--<Button text="{i18n>PerformCIC}">-->
				<!--	<layoutData>-->
				<!--		<l:GridData span="XL8 L8 M12 S12"/>-->
				<!--	</layoutData>-->
				<!--</Button>-->
				<Link emphasized="true" text="{i18n>PerformCIC}" href="https://b2b.acpt.toyota.ca/CICWeb/" target="_blank"/>
				<core:Title text=""/>
				<Label text="{i18n>AgreementNumber}"/>
				<Text text="{HeadSetData>/AgreementNumber}"/>
			</f:content>
		</f:SimpleForm>
		<VBox visible="{DateModel>/RepairdDetailVisible}">
			<f:SimpleForm visible="{DateModel>/RepairdDetailVisible}" editable="{DateModel>/oFormEdit}" title="{i18n>RepairDetails}"
				layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
				emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
				<f:content>
					<core:Title text=""/>
					<Label text="{i18n>OFP}"/>
					<Input id="idOFP" required="{DateModel>/ofpRequired}" change="onChangeOFP"
						value="{path:'HeadSetData>/OFP', type : 'sap.ui.model.type.String', constraints : { minLength : 2, maxLength : 40 }}"
						editable="{DateModel>/oFormEdit}" visible="{DateModel>/ofpEnabled}" maxLength="40"/>
					<Label text="{i18n>MainOp}"/>
					<Input id="idMainOps" maxLength="9" change="onChangeMainOp"
						value="{path:'HeadSetData>/MainOpsCode', type : 'sap.ui.model.type.String', constraints : { minLength : 1, maxLength : 9 }}"
						required="{DateModel>/oMainOpsReq}" editable="{DateModel>/oFormEdit}" visible="{DateModel>/oMainOps}"/>
					<Label text="{i18n>BatteryTestCode}"/>
					<Input id="idBattTestCOde" maxLength="14" value="{HeadSetData>/BatteryTestCode}" editable="{DateModel>/oFormEdit}"
						enabled="{DateModel>/oBatteryTestEnable}"/>
					<Label text="{i18n>Condition}"/>
					<TextArea maxLength="100" required="true" id="idCondition" editable="{DateModel>/oFormEdit}"
						value="{path:'HeadSetData>/ZCondition', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 100 }}"></TextArea>
					<Label text=""/>
					<Link text="{i18n>ViewCCRAbbreviations}" press="onPressAbbr"/>
					<core:Title text=""/>
					<Label text="{i18n>OFPDescription}"/>
					<Text text="{path:'LocalDataModel>/OFPDescription'}" visible="{DateModel>/ofpEnabled}"/>
					<Label text="{i18n>MainOpDescription}"/>
					<Text text="{path:'LocalDataModel>/MainOpsCodeDescription'}" visible="{DateModel>/oMainOps}"/>
					<Label text="{i18n>FieldActionCode}"/>
					<Input id="idFieldActionInput" maxLength="10" required="{path:'DateModel>/oFieldActionInput'}"
						value="{path:'HeadSetData>/FieldActionReference', type : 'sap.ui.model.type.String', constraints : { minLength : 2, maxLength : 10 }}"
						editable="{DateModel>/oFormEdit}" enabled="{DateModel>/oFieldActionInput}"/>
					<Label text="{i18n>Cause}"/>
					<TextArea maxLength="100" required="true" editable="{DateModel>/oFormEdit}" id="idCause"
						value="{path:'HeadSetData>/Cause',type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 100 }}"></TextArea>
					<core:Title text=""/>
					<Label text="{i18n>AppliedCoverage}"/>
					<Text text="{HeadSetData>/AppliedWarrantyCoverage}"/>
					<Label text="{i18n>T1Code}"/>
					<ComboBox id="idT1Field" required="{DateModel>/enabledT1}" selectionChange="onChangeT1" showSecondaryValues="true"
						items="{ path: '/Zt1DescSrchhelpSet' }" filterSecondaryValues="true"
						value="{path:'HeadSetData>/T1WarrantyCodes', type : 'sap.ui.model.type.String', constraints : { minLength : 1, maxLength : 140 } }"
						selectedKey="{path:'HeadSetData>/T1WarrantyCodes'}" visible="{DateModel>/enabledT1}" editable="{DateModel>/oFormEdit}">
						<core:ListItem key="{T1}" text="{path:'T1'}" additionalText="{T1Text}"/>
					</ComboBox>
					<!--<Select selectedKey="{HeadSetData>/T1WarrantyCodes}" items="{ path: '/Zt1DescSrchhelpSet' }" enabled="{DateModel>/oFormEdit}">-->
					<!--	<core:Item key="{T1}" text="{T1} - {T1Text}"/>-->
					<!--</Select>-->
					<Label text="{i18n>T2Code}"/>
					<ComboBox id="idT2Field" required="{DateModel>/enabledT2}" selectionChange="onChangeT2" showSecondaryValues="true"
						items="{ path: '/Zt2DescSrchhelpSet' }" filterSecondaryValues="true"
						value="{path:'HeadSetData>/T2WarrantyCodes', type : 'sap.ui.model.type.String', constraints : { minLength : 1, maxLength : 140 } }"
						selectedKey="{path:'HeadSetData>/T2WarrantyCodes'}" visible="{DateModel>/enabledT2}" editable="{DateModel>/oFormEdit}">
						<core:ListItem key="{path:'T2'}" text="{T2}" additionalText="{T2Text}"/>
					</ComboBox>
					<!--<Select selectedKey="{HeadSetData>/T2WarrantyCodes}" items="{ path: '/Zt2DescSrchhelpSet' }" enabled="{DateModel>/oFormEdit}">-->
					<!--	<core:Item key="{T2}" text="{T2} - {T2Text}"/>-->
					<!--</Select>-->
					<Label text="{i18n>Remedy}"/>
					<TextArea id="idRemedy" required="true" editable="{DateModel>/oFormEdit}" maxLength="100"
						value="{path:'HeadSetData>/Remedy', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 100 }}"></TextArea>
				</f:content>
			</f:SimpleForm>
		</VBox>
		<f:SimpleForm visible="{DateModel>/AcA1}" editable="{DateModel>/oFormEdit}" title="Accessory Installation/Purchase"
			layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
			emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<Label text="{i18n>InstallDate}"/>
				<DatePicker id="idAccDate" class="clDatePicker" visible="{DateModel>/AcA1}" required="{DateModel>/AcA1}"
					value="{path:'HeadSetData>/AccessoryInstallDate', type: 'sap.ui.model.type.Date', formatOptions: { style: 'medium', pattern: 'YYYY-MM-dd'}}"
					editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="{i18n>InstallOdometer}"/>
				<Input id="idInsOdo" required="{DateModel>/AcA1}" visible="{DateModel>/AcA1}" editable="{DateModel>/oFormEdit}"
					value="{path:'HeadSetData>/AccessoryInstallOdometer', type : 'sap.ui.model.type.String', constraints : { minLength : 1, maxLength : 6 }}"/>
				<core:Title text=""/>
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm visible="{DateModel>/P1p2}" editable="{DateModel>/oFormEdit}" title="Original/Previous Installation"
			layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
			emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<Label text="{i18n>PreviousROInvoice}"/>
				<Input required="{DateModel>/oPrevInvNumReq}" id="idPreInvNum"
					value="{path:'HeadSetData>/PreviousROInvoice', type : 'sap.ui.model.type.String', constraints : { minLength : 1, maxLength : 20 }}"
					editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="{i18n>ROInvoiceDate}"/>
				<DatePicker class="clDatePicker" id="idPrInvDate" required="{DateModel>/oPrevInvDateReq}"
					value="{path:'HeadSetData>/PreviousROInvoiceDate', type: 'sap.ui.model.type.Date', formatOptions: { pattern: 'YYYY-MM-dd'}}"
					editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="{i18n>ROOdometer}"/>
				<Input visible="{DateModel>/PreroOdometerVisible}" value="{path:'HeadSetData>/PreviousROOdometer'}" editable="{DateModel>/oFormEdit}"/>
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm editable="{DateModel>/oFormEdit}" title="{i18n>ClaimComments}" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3"
			labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="1" emptySpanL="1" emptySpanM="1" emptySpanS="0" columnsXL="1"
			columnsL="1" columnsM="1" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<TextArea height="8rem" value="{HeadSetData>/HeadText}" editable="{DateModel>/commentEditable}" enabled="{DateModel>/oFormEdit}">
					<layoutData>
						<l:GridData span="XL8 L8 M8 S12"/>
					</layoutData>
				</TextArea>
				<Button text="Add" press="onAddComment" enabled="{DateModel>/oFormEdit}">
					<layoutData>
						<l:GridData span="XL2 L2 M3 S4"/>
					</layoutData>
				</Button>
			</f:content>
		</f:SimpleForm>
	</VBox>
	<VBox>
		<Panel width="auto" class="sapUiSmallMarginBottom" accessibleRole="Region">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="{i18n>SupportingDocuments}"/>
				</Toolbar>
			</headerToolbar>
			<content>
				<f:SimpleForm editable="{DateModel>/oFormEdit}" title="" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3"
					labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="0" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
					singleContainerFullSize="false">
					<f:content>
						<!--<UploadCollection sameFilenameAllowed="false" instantUpload="true" uploadEnabled="{LocalDataModel>/UploadEnable}" -->
						<!--change="onUplaodChange" id="UploadSupportingDoc" multiple="false" -->
						<!--uploadComplete="onUploadComplete" fileDeleted="onFileDeleted"-->
						<!-- items="{ path: 'LocalDataModel>/HeadAtchmentData'}">-->
						<UploadCollection id="idHeadAttachment" maximumFileSize="10" fileSizeExceed="onFileSizeExceed"
							uploadEnabled="{LocalDataModel>/UploadEnable}" change="onUplaodChange" uploadComplete="onUploadComplete" fileDeleted="onFileDeleted"
							instantUpload="true" busy="{LocalDataModel>/IndicatorState}" sameFilenameAllowed="false"
							items="{ path: 'LocalDataModel>/HeadAtchmentData', templateShareable: false }">
							<toolbar>
								<OverflowToolbar>
									<Breadcrumbs id="breadcrumbsSupportingDoc" currentLocationText="{LocalDataModel>/currentLocationText}" links="{LocalDataModel>/history}">
										<Link press="onBreadcrumbPress" text="{LocalDataModel>fileName}"/>
									</Breadcrumbs>
									<ToolbarSpacer/>
									<UploadCollectionToolbarPlaceholder/>
								</OverflowToolbar>
							</toolbar>
							<items>
								<!--<UploadCollectionItem   documentId="{LocalDataModel>DOC_ID}" fileName="{LocalDataModel>FileName}"-->
								<!--	mimeType="{LocalDataModel>MIMETYPE}" thumbnailUrl="{LocalDataModel>url}" url="{LocalDataModel>URI}" enableEdit="{settings>/enableEdit}"-->
								<!--	enableDelete="{settings>/enableDelete}" visibleDelete="{settings>/visibleDelete}" visibleEdit="{settings>/visibleEdit}"></UploadCollectionItem>-->
								<UploadCollectionItem documentId="{LocalDataModel>DOC_ID}" fileName="{LocalDataModel>FileName}" mimeType="{LocalDataModel>MIMETYPE}"
									thumbnailUrl="{LocalDataModel>url}" url="{LocalDataModel>URI}" enableEdit="false" enableDelete="{LocalDataModel>/UploadEnable}"
									visibleDelete="{settings>/visibleDelete}" visibleEdit="false"></UploadCollectionItem>
							</items>
						</UploadCollection>
					</f:content>
				</f:SimpleForm>
			</content>
		</Panel>
		<Panel width="auto" class="sapUiSmallMarginBottom" accessibleRole="Region">
			<content>
				<table:Table class="classRCPTable" selectionMode="Single" rows="{path:'LocalDataModel>/ClaimSum'}" visibleRowCount="5"
					rowSelectionChange="onNavigate" selectionBehavior="Row" showNoData="true" threshold="5" enableBusyIndicator="true">
					<table:toolbar>
						<Toolbar>
							<Title text="{i18n>ClaimAmountSummary}" level="H2"/>
							<ToolbarSpacer/>
							<!--<Button tooltip="View Settings" icon="sap-icon://drop-down-list" press="handleViewSettingsDialogButtonPressed"/>-->
						</Toolbar>
					</table:toolbar>
					<table:columns autoResizable="true">
						<table:Column width="17rem">
							<table:multiLabels>
								<Label text="{i18n>Totalsincludedmarkupdiscounts}" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="{path:'LocalDataModel>ItemType', formatter: 'zclaimProcessing.utils.formatter.fnFormatText'}" wrapping="false"/>
							</table:template>
						</table:Column>
						<table:Column width="11rem">
							<table:multiLabels>
								<Label text="{i18n>DealerClaimedAmount}" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="${path:'LocalDataModel>DealerClaim', formatter:'zclaimProcessing.utils.formatter.fnFormatDecimal'}" wrapping="false"/>
							</table:template>
						</table:Column>
						<table:Column width="11rem">
							<table:multiLabels>
								<Label text="{i18n>TCIApprovedAmount}" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="${path:'LocalDataModel>TCIAppr', formatter:'zclaimProcessing.utils.formatter.fnFormatDecimal'}" wrapping="false"/>
							</table:template>
						</table:Column>
						<table:Column width="11rem">
							<table:multiLabels>
								<Label text="{i18n>DifferenceCAD}" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="${path:'LocalDataModel>Diff', formatter:'zclaimProcessing.utils.formatter.fnFormatDecimal'}" wrapping="false"/>
							</table:template>
						</table:Column>
						<table:Column width="8rem" headerSpan="{multiHeaderConfig>/multiheader2}">
							<table:multiLabels>
								<Label text="{i18n>GSTHST}" textAlign="Center" width="100%"/>
								<Label text="{i18n>Amount}" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="${path:'LocalDataModel>GSTHSTAmt', formatter:'zclaimProcessing.utils.formatter.fnFormatDecimal'}" wrapping="false"/>
							</table:template>
						</table:Column>
						<table:Column width="8rem">
							<table:multiLabels>
								<Label text="{i18n>GSTHST}" textAlign="Center" width="100%"/>
								<Label text="{i18n>Tax}" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="{LocalDataModel>GSTHSTRate}" wrapping="false"/>
							</table:template>
						</table:Column>
						<table:Column width="8rem" headerSpan="{multiHeaderConfig>/multiheader2}">
							<table:multiLabels>
								<Label text="{i18n>PSTQST}" textAlign="Center" width="100%"/>
								<Label text="Amount" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="${path:'LocalDataModel>PSTQSTAmt', formatter:'zclaimProcessing.utils.formatter.fnFormatDecimal'}" wrapping="false"/>
							</table:template>
						</table:Column>
						<table:Column width="8rem">
							<table:multiLabels>
								<Label text="{i18n>PSTQST}" textAlign="Center" width="100%"/>
								<Label text="{i18n>Tax}" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="{LocalDataModel>PSTQSTRate}" wrapping="false"/>
							</table:template>
						</table:Column>
						<table:Column width="17rem">
							<table:multiLabels>
								<Label text="{i18n>TCIApprovedAmountIncludingTax}" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="${path:'LocalDataModel>TCIApprInclTax', formatter:'zclaimProcessing.utils.formatter.fnFormatDecimal'}" wrapping="false"/>
							</table:template>
						</table:Column>
					</table:columns>
				</table:Table>
			</content>
			<HBox alignContent="Center" justifyContent="Center" class="sapUiTinyMarginTop sapUiTinyMarginBottom">
				<Button class="sapUiTinyMarginBeginEnd" press="onPressBack" text="{i18n>BackList}" type="Back"/>
				<Button class="sapUiTinyMarginBeginEnd" press="onCancelClaim" enabled="{LocalDataModel>/CancelEnable}" text="{i18n>CancelClaim}"
					type="Reject"/>
				<Button class="sapUiTinyMarginBeginEnd" text="{i18n>Print}" enabled="{LocalDataModel>/PrintEnable}" press="onPressPrint"/>
				<Button class="sapUiTinyMarginBeginEnd" press="onStep01Next" enabled="{LocalDataModel>/step01Next}" text="{i18n>Next}" type="Emphasized"/>
			</HBox>
		</Panel>
	</VBox>
	<!--<VBox width="100%" class="classCheck"></VBox>-->
</core:FragmentDefinition>