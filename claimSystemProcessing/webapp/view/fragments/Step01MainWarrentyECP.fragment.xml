				<Label text="{i18n>Agreement Number}"/>
				<Text text=""/>
			</f:content>
		</f:SimpleForm>
		<VBox visible="{DateModel>/RepairdDetailVisible}">
			<f:SimpleForm visible="{DateModel>/RepairdDetailVisible}" id="idpart01Form" editable="{DateModel>/oFormEdit}" title="{i18n>RepairDetails}"
				layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
				emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
				<f:content>
					<core:Title text=""/>
					<Label text="{i18n>OFP}"/>
					<Input value="{path:'HeadSetData>/OFP'}" editable="{DateModel>/oFormEdit}"/>
					<Label text="{i18n>MainOp}"/>
					<Input value="{path:'HeadSetData>/MainOpsCode'}" editable="{DateModel>/oFormEdit}"/>
					<Label text="{i18n>BatteryTestCode}"/>
					<Input value="{HeadSetData>/BatteryTestCode}" editable="{DateModel>/oFormEdit}"/>
					<Label text="{i18n>Condition}"/>
					<TextArea required="true" editable="{DateModel>/oFormEdit}"
						value="{path:'HeadSetData>/ZCondition', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 100 }}"></TextArea>
					<core:Title text=""/>
					<Label text="{i18n>OFPDescription}"/>
					<Text text="{path:'LocalDataModel>/OFPDescription'}"/>
					<Label text="{i18n>MainOpDescription}"/>
					<Text text="{path:'LocalDataModel>/MainOpsCodeDescription'}"/>
					<Label text="{i18n>FieldActionCode}"/>
					<Input required="{path:'DateModel>/oFieldActionInput'}" value="{path:'HeadSetData>/FieldActionReference'}" editable="{DateModel>/oFormEdit}"
						enabled="{DateModel>/oFieldActionInput}" change="onFieldActionInput"/>
					<Label text="{i18n>Cause}"/>
					<TextArea required="true" editable="{DateModel>/oFormEdit}"
						value="{path:'HeadSetData>/Cause',type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 100 }}"></TextArea>
					<core:Title text=""/>
					<Label text="{i18n>AppliedCoverage}"/>
					<Text text="{HeadSetData>/AppliedWarrantyCoverage}"/>
					<Label text="{i18n>T1 Code}"/>
					<ComboBox showSecondaryValues="true" required="true" items="{ path: '/Zt1DescSrchhelpSet' }" selectedKey="{HeadSetData>/T1WarrantyCodes}"
						enabled="{DateModel>/oFormEdit}"
						value="{path:'HeadSetData>/T1WarrantyCodes', type : 'sap.ui.model.type.String', constraints : { minLength : 2, maxLength : 100 }}">
						<core:ListItem key="{T1}" text="{path:'T1', constraints : { minLength : 2, maxLength : 100 }}" additionalText="{T1Text}"/>
					</ComboBox>
					<!--<Select selectedKey="{HeadSetData>/T1WarrantyCodes}" items="{ path: '/Zt1DescSrchhelpSet' }" enabled="{DateModel>/oFormEdit}">-->
					<!--	<core:Item key="{T1}" text="{T1} - {T1Text}"/>-->
					<!--</Select>-->
					<Label text="{i18n>T2Code}"/>
					<ComboBox required="true" showSecondaryValues="true" items="{ path: '/Zt2DescSrchhelpSet' }" selectedKey="{HeadSetData>/T2WarrantyCodes}"
						enabled="{DateModel>/oFormEdit}"
						value="{path:'HeadSetData>/T2WarrantyCodes', type : 'sap.ui.model.type.String', constraints : { minLength : 2, maxLength : 100 }}">
						<core:ListItem key="{path:'T2', constraints : { minLength : 2, maxLength : 100 }}" text="{T2}" additionalText="{T2Text}"/>
					</ComboBox>
					<!--<Select selectedKey="{HeadSetData>/T2WarrantyCodes}" items="{ path: '/Zt2DescSrchhelpSet' }" enabled="{DateModel>/oFormEdit}">-->
					<!--	<core:Item key="{T2}" text="{T2} - {T2Text}"/>-->
					<!--</Select>-->
