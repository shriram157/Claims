<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:u="sap.ui.unified" xmlns:l="sap.ui.layout"
	xmlns="sap.m" xmlns:table="sap.ui.table" xmlns:custom="zclaimProcessing.control">
	<VBox class="sapUiSmallMargin clFormTitleGap">
		<Panel width="auto" class="sapUiSmallMarginBottom" accessibleRole="Region">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="{i18n>ClaimDetails}"/>
					<ToolbarSpacer/>
					<Title text="{i18n>TCIClaimNumber}: {HeadSetData>/NumberOfWarrantyClaim}" textAlign="End"/>
				</Toolbar>
			</headerToolbar>
			<f:SimpleForm editable="true" layout="ResponsiveGridLayout" title="" labelSpanXL="6" labelSpanL="6" labelSpanM="12" labelSpanS="12"
				emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="4" columnsL="4" columnsM="6">
				<f:content>
					<core:Title text=""/>
					<Label text="{i18n>ClaimType}"/>
					<ComboBox showSecondaryValues="true" required="true" items="{path:'LocalDataModel>/ClaimGroupSet'}" selectionChange="onSelectClaimTpe"
						selectedKey="{HeadSetData>/ClaimType}" enabled="{DateModel>/claimTypeEn}" valueState="{DateModel>/claimTypeState}"
						value="{path:'HeadSetData>/ClaimType', type : 'sap.ui.model.type.String', constraints : { minLength : 2, maxLength : 50 } }">
						<core:ListItem text="{LocalDataModel>ALMClaimType}" key="{LocalDataModel>ALMClaimType}" additionalText="{LocalDataModel>ALMClaimTypeDes}"/>
					</ComboBox>
					<core:Title text=""/>
					<Label text="{i18n>FirstSubmissionDate}"/>
					<Text
						text="{ path: 'HeadSetData>/ReferenceDate', type : 'sap.ui.model.type.Date', formatOptions : { source: { pattern: 'dd-MM-yyyy' }, pattern: 'dd-MM-yyyy' }, constraints : { minimum : '01-01-1990' } }"/>
					<Label text="{i18n>ClaimAge}"/>
					<Text text="{HeadSetData>/ClaimAge}"/>
					<Label text="{i18n>LastModifiedby}"/>
					<Text text="{HeadSetData>/NameOfPersonRespWhoChangedObj}"/>
					<core:Title text=""/>
					<Label text="{i18n>LastSubmissionDate}"/>
					<Text
						text="{ path: 'HeadSetData>/DateOfApplication', type: 'sap.ui.model.type.Date', constraints: {displayFormat: 'Date' }, formatOptions: { style: 'medium', strictParsing: true , UTC: true}}"/>
					<Label text="Final Processed Date"/>
					<Text
						text="{ path: 'HeadSetData>/FinalProcdDate', type: 'sap.ui.model.type.Date', constraints: {displayFormat: 'Date' }, formatOptions: { style: 'medium', strictParsing: true , UTC: true} }"/>
					<core:Title text=""/>
					<Button text="{i18n>SaveClaim}" class="sapUiSmallMarginEnd" type="Emphasized" press="onSaveClaim" visible="{DateModel>/saveClaimSt}">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					<Button text="{i18n>Edit}" class="sapUiSmallMarginEnd" type="Emphasized" press="onEditClaim" visible="{DateModel>/claimEditSt}">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					<Button text="{i18n>SaveClaim}" class="sapUiSmallMarginEnd" enabled="{DateModel>/updateEnable}" type="Emphasized" press="onUpdateClaim"
						visible="{DateModel>/updateClaimSt}">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
					<Button text="{i18n>CopytoClaim}" type="Default" press="onCopyClaim">
						<layoutData>
							<l:GridData span="XL8 L8 M12 S12"/>
						</layoutData>
					</Button>
				</f:content>
			</f:SimpleForm>
		</Panel>
		<f:SimpleForm id="idClaimMainForm" editable="{DateModel>/oFormEdit}" title="Repair Order Information" layout="ResponsiveGridLayout"
			labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0"
			emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<Label text="{i18n>DealerName}"/>
				<Text text="{HeadSetData>/CustomerFullName}"/>
				<Label text="{i18n>DealerClaim}"/>
				<Input
					value="{path:'HeadSetData>/ExternalNumberOfClaim', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 40 } }"
					required="true" id="idDealerClaim" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="{i18n>RepairDate}"/>
				<DatePicker required="true" value="{path:'HeadSetData>/RepairDate',type : 'sap.ui.model.type.Date'}" editable="{DateModel>/oFormEdit}"/>
				<Label text="{i18n>DealerContact}"/>
				<Input required="true"
					value="{path:'HeadSetData>/DealerContact', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 30 }}"
					id="idDealerContact" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="{i18n>RepairOrder}"/>
				<Input required="true"
					value="{path:'HeadSetData>/RepairOrderNumberExternal', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 10 }}"
					id="idRepairOrder" editable="{DateModel>/oFormEdit}"/>
				<Label text="{i18n>ROOdometer}"/>
				<custom:CustomInput
					value="{path:'HeadSetData>/Odometer', type : 'sap.ui.model.type.Number', constraints : { minLength : 2, maxLength : 7 }}"
					required="{DateModel>/OdometerReq}" visible="{DateModel>/OdometerReq}" id="idOdometer" editable="{DateModel>/OdometerReq}"/>
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm editable="" id="idVehicleInfo" title="{i18n>VehicleInformationToyotaCorolaLECVT}" layout="ResponsiveGridLayout"
			labelSpanXL="6" labelSpanL="6" labelSpanM="6" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0"
			emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<RadioButtonGroup selectedIndex="0" columns="2" id="idRequestType" select="onSelectRequestType" editable="{DateModel>/oFormEdit}">
					<buttons>
						<RadioButton groupName="E" text="{i18n>VIN}"/>
						<RadioButton groupName="E" text="{i18n>NonVehicle}"/>
					</buttons>
				</RadioButtonGroup>
				<Input required="{DateModel>/OdometerReq}"
					value="{path:'HeadSetData>/ExternalObjectNumber', type : 'sap.ui.model.type.String', constraints : { minLength : 16, maxLength : 17 }}"
					visible="{DateModel>/OdometerReq}" id="idVinNum" change="onEnterVIN" editable="{DateModel>/oFormEdit}" />
				<core:Title text=""/>
				<Label text="{i18n>RegDate}"/>
				<Text text="{HeadSetData>/RegDate}"/>
				<Label text="{i18n>VehicleAge}"/>
				<Text text="" id="idVehicleAge"/>
				<core:Title text=""/>
				<Label text="{i18n>ForeignVIN}"/>
				<Text text="{HeadSetData>/ForeignVINIndicator}" id="idForeignVin"/>
				<Button icon="sap-icon://message-warning" type="Transparent" press="onPressForeignVin" class="clWarning"/>
				<Label text="{i18n>WrittenOff}"/>
				<Text text="{HeadSetData>/WrittenOffCode}" id="idWrittenoff"/>
				<Button icon="sap-icon://message-warning" type="Transparent" press="onPressWrittenOff" class="clWarning"/>
				<Label text="{i18n>SpecialVINReview}"/>
				<Text text="{HeadSetData>/SpecialVINReview}" id="idSpecialVin"/>
				<Button icon="sap-icon://message-warning" type="Transparent" press="onPressSpecialVin" class="clWarning"/>
			</f:content>
		</f:SimpleForm>
		<VBox visible="false">
			<f:SimpleForm editable="true" id="idShipmentInfo" title="{i18n>ShipmentInformation}" layout="ResponsiveGridLayout" labelSpanXL="6"
				labelSpanL="6" labelSpanM="6" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0"
				columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
				<f:content>
					<Label text="{i18n>Delivering Carrier}"/>
					<ComboBox required="true" items="" selectedKey="" value="">
						<core:ListItem key="" text="United Road"/>
						<core:ListItem key="" text="Mc Gill"/>
					</ComboBox>
					<Label text="{i18n>Probill #}"/>
					<Input value="" required="true"/>
					<Label text="{i18n>Delivery Date}"/>
					<DatePicker required="true" value=""/>
					<Label text="{i18n>Delivery}"/>
					<ComboBox required="true" items="" selectedKey="" value="">
						<core:ListItem key="" text="Attended"/>
						<core:ListItem key="" text="Unattended"/>
					</ComboBox>
				</f:content>
			</f:SimpleForm>
			<HBox justifyContent="End">
				<Button text="Add Damage Line" type="Emphasized" enabled=""/>
				<Button text="Update Damage Line" class="sapUiTinyMarginBeginEnd" enabled=""/>
				<Button text="Delete Damage Line" class="sapUiTinyMarginBeginEnd" type="Reject" enabled=""/>
			</HBox>
			<VBox id="idDamageArea" visible="" class="clBgColor sapUiSmallMarginTop" wrap="Wrap" width="100%">
				<f:SimpleForm editable="true" labelSpanL="4" width="100%" title="" labelSpanXL="12" layout="ResponsiveGridLayout" emptySpanXL="1"
					emptySpanL="1" columnsM="1" columnsL="1" columnsXL="1">
					<f:content>
						<Label text="{i18n>Area}"/>
						<ComboBox showSecondaryValues="true" required="true" items="" selectedKey="" value="">
							<core:ListItem key="" text="01" additionalText="Anttena"/>
							<core:ListItem key="" text="02" additionalText="Battery"/>
						</ComboBox>
						<Label text="{i18n>Type}"/>
						<ComboBox showSecondaryValues="true" required="true" items="" selectedKey="" value="">
							<core:ListItem key="" text="01" additionalText="Bent"/>
							<core:ListItem key="" text="02" additionalText="Broken"/>
						</ComboBox>
						<Label text="{i18n>Severity}"/>
						<ComboBox showSecondaryValues="true" required="true" items="" selectedKey="" value="">
							<core:ListItem key="" text="01" additionalText="Less than"/>
							<core:ListItem key="" text="02" additionalText="Including"/>
						</ComboBox>
					</f:content>
				</f:SimpleForm>
			</VBox>
			<Table id="idDamageDetailTable" mode="None" items="{LocalDataModel>/PartDetailList}" class="sapUiMediumMarginTop"
				selectionChange="OnSelectionChange">
				<columns>
					<Column>
						<Text text="{i18n>Line}"/>
					</Column>
					<Column>
						<Text text="{i18n>AreaCode}"/>
					</Column>
					<Column>
						<Text text="{i18n>AreaDescription}"/>
					</Column>
					<Column>
						<Text text="{i18n>TypeCode}"/>
					</Column>
					<Column>
						<Text text="{i18n>TypeDescription}"/>
					</Column>
					<Column>
						<Text text="{i18n>SeverityCode}"/>
					</Column>
					<Column>
						<Text text="{i18n>SeverityDescription}"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<cells>
							<Text text=""/>
						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</VBox>
		<Table id="idECPAGR" headerText="{i18n>ActiveAgreeement}" mode="SingleSelectLeft" items="{path:'LocalDataModel>/AgreementDataECP'}"
			class="sapUiMediumMarginTop" selectionChange="onNavigate" visible="{DateModel>/oECPfields}">
			<columns>
				<Column>
					<Label text="{i18n>ECPAgreement}" textAlign="Center" width="100%"/>
				</Column>
				<Column>
					<Label text="{i18n>PlanCode}" textAlign="Center" width="100%"/>
				</Column>
				<Column>
					<Label text="{i18n>Status}" textAlign="Center" width="100%"/>
				</Column>
				<Column>
					<Label text="{i18n>StatusDate}" textAlign="Center" width="100%"/>
				</Column>
				<Column>
					<Label text="{i18n>ExpiryDate}" textAlign="Center" width="100%"/>
				</Column>
				<Column>
					<Label text="{i18n>ExpiryKM}" textAlign="Center" width="100%"/>
				</Column>
				<Column></Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<Text text="{LocalDataModel>AgreementNumber}" wrapping="true"/>
						<Text text="{LocalDataModel>PlanType}" wrapping="true"/>
						<Text text="{LocalDataModel>bcc_agr_st_cd}" wrapping="true"/>
						<Text
							text="{path:'LocalDataModel>StatusDate', type: 'sap.ui.model.type.Date', constraints: { displayFormat: 'Date' }, formatOptions: { style: 'medium', strictParsing: true , UTC: true}}"
							wrapping="true"/>
						<Text
							text="{path:'LocalDataModel>AgreementthruDate', type: 'sap.ui.model.type.Date', constraints: { displayFormat: 'Date' }, formatOptions: { style: 'medium', strictParsing: true , UTC: true}}"
							wrapping="true"/>
						<Text text="{LocalDataModel>AgreementthruKMreading}" wrapping="true"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
