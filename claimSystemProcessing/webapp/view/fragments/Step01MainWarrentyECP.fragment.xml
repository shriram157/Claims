			<!--			<l:GridData span="XL4 L4 M6 S12"/>-->
			<!--		</layoutData>-->
			<!--	</Button>-->
			<!--</HBox>-->
		</Panel>
		<f:SimpleForm id="idClaimMainForm" editable="" title="Repair Order Information" layout="ResponsiveGridLayout" labelSpanXL="12"
			labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0"
			columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<Label text="Dealer Name"/>
				<Text text="{HeadSetData>/CustomerFullName}"/>
				<Label text="Dealer Claim"/>
				<Input
					value="{path:'HeadSetData>/ExternalNumberOfClaim', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 40 } }"
					required="true" id="idDealerClaim" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="Repair Date"/>
				<DatePicker required="true" value="{path:'HeadSetData>/RepairDate',type : 'sap.ui.model.type.Date'}" editable="{DateModel>/oFormEdit}"/>
				<Label text="Dealer Contact"/>
				<Input required="true"
					value="{path:'HeadSetData>/DealerContact', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 30 }}"
					id="idDealerContact" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="Repair Order"/>
				<Input required="true"
					value="{path:'HeadSetData>/RepairOrderNumberExternal', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 10 }}"
					id="idRepairOrder" editable="{DateModel>/oFormEdit}"/>
				<Label text="R.O. Odometer"/>
				<Input type="Number" value="{path:'HeadSetData>/Odometer', type : 'sap.ui.model.type.Number', constraints : { minLength : 2, maxLength : 7 }}"
					required="{DateModel>/OdometerReq}" visible="{DateModel>/OdometerReq}" id="idOdometer" editable="{DateModel>/OdometerReq}"/>
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm editable="" id="idVehicleInfo" title="Vehicle Information - Toyota Corola LE CVT 2018" layout="ResponsiveGridLayout"
			labelSpanXL="6" labelSpanL="6" labelSpanM="6" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0"
			emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<RadioButtonGroup selectedIndex="0" columns="2" id="idRequestType" select="onSelectRequestType" editable="{DateModel>/oFormEdit}">
					<buttons>
						<RadioButton groupName="E" text="VIN"/>
						<RadioButton groupName="E" text=" Non Vehicle"/>
					</buttons>
				</RadioButtonGroup>
				<Input
					value="{path:'HeadSetData>/ExternalObjectNumber', type : 'sap.ui.model.type.String', constraints : { minLength : 16, maxLength : 17 }}"
					required="true" visible="{DateModel>/OdometerReq}" id="idVinNum" change="onEnterVIN" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="Reg. Date"/>
				<Text text="{HeadSetData>/RegDate}"/>
				<Label text="Vehicle Age"/>
				<Text text="" id="idVehicleAge"/>
				<core:Title text=""/>
				<Label text="Foreign VIN"/>
				<Text text="{HeadSetData>/ForeignVINIndicator}" id="idForeignVin"/>
				<Button icon="sap-icon://message-warning" type="Transparent" press="onPressForeignVin" class="clWarning"/>
				<Label text="Written-Off"/>
				<Text text="{HeadSetData>/WrittenOffCode}" id="idWrittenoff"/>
				<Button icon="sap-icon://message-warning" type="Transparent" press="onPressWrittenOff" class="clWarning"/>
				<Label text="Special VIN Review"/>
				<Text text="{HeadSetData>/SpecialVINReview}" id="idSpecialVin"/>
				<Button icon="sap-icon://message-warning" type="Transparent" press="onPressSpecialVin" class="clWarning"/>
			</f:content>
		</f:SimpleForm>
		<table:Table id="idECPAGR" class="classRCPTable" selectionMode="Single" rows="{path:'LocalDataModel>/AgreementDataECP'}"
			rowSelectionChange="onNavigate" selectionBehavior="Row" showNoData="true" threshold="4" visibleRowCount="4" enableBusyIndicator="true"
			visible="{DateModel>/oECPfields}">
			<table:toolbar>
				<Toolbar>
					<Title text="Active Agreeement(s)" level="H2"/>
				</Toolbar>
			</table:toolbar>
			<table:columns autoResizable="true">
				<table:Column width="9rem">
					<table:multiLabels>
						<Label text="ECP Agreement" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{LocalDataModel>AgreementNumber}" wrapping="true"/>
					</table:template>
				</table:Column>
				<table:Column width="9rem">
					<table:multiLabels>
						<Label text="Plan Code" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{LocalDataModel>PlanType}" wrapping="true"/>
					</table:template>
				</table:Column>
				<table:Column width="9rem">
					<table:multiLabels>
						<Label text="Status" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{LocalDataModel>bcc_agr_st_cd}" wrapping="true"/>
					</table:template>
				</table:Column>
				<table:Column width="17rem">
					<table:multiLabels>
						<Label text="Status Date" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text
							text="{path:'LocalDataModel>StatusDate', type: 'sap.ui.model.type.Date', constraints: { displayFormat: 'Date' }, formatOptions: { style: 'medium', strictParsing: true , UTC: true}}"
							wrapping="true"/>
					</table:template>
				</table:Column>
				<table:Column width="17rem">
					<table:multiLabels>
						<Label text="Expiry Date" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text
							text="{path:'LocalDataModel>AgreementthruDate', type: 'sap.ui.model.type.Date', constraints: { displayFormat: 'Date' }, formatOptions: { style: 'medium', strictParsing: true , UTC: true}}"
							wrapping="true"/>
					</table:template>
				</table:Column>
				<table:Column width="17rem">
					<table:multiLabels>
						<Label text="Expiry KM" textAlign="Center" width="100%"/>
					</table:multiLabels>
					<table:template>
						<Text text="{LocalDataModel>AgreementthruKMreading}" wrapping="true"/>
					</table:template>
				</table:Column>
			</table:columns>
		</table:Table>
		<f:SimpleForm editable="" title="" layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12"
			adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="5" columnsL="5" columnsM="3"
			singleContainerFullSize="false" visible="{DateModel>/oECPfields}">
			<f:content>
				<core:Title text=""/>
				<Label text="Client's Last Name"/>
				<Input value="" required="true"/>
				<core:Title text=""/>
				<Label text="Client's Last Name"/>
				<Input value="" required="true"/>
				<core:Title text=""/>
				<Label text=" "/>
				<Button text="Lookup ECP Agreements"/>
				<core:Title text=""/>
				<Label text=" "/>
				<Button text="Perform CIC"/>
				<core:Title text=""/>
				<Label text="Agreement Number"/>
				<Text text=""/>
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm id="idpart01Form" editable="{DateModel>/oFormEdit}" title="Repair Details" layout="ResponsiveGridLayout" labelSpanXL="12"
			labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0"
			columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<Label text="OFP"/>
				<Input value="{path:'HeadSetData>/OFP'}" editable="{DateModel>/oFormEdit}"/>
				<Label text="Main Op"/>
				<Input value="{path:'HeadSetData>/MainOpsCode'}" editable="{DateModel>/oFormEdit}"/>
				<Label text="Battery Test Code"/>
				<Input value="{HeadSetData>/BatteryTestCode}" editable="{DateModel>/oFormEdit}"/>
				<Label text="Condition"/>
				<TextArea required="true" editable="{DateModel>/oFormEdit}"
					value="{path:'HeadSetData>/ZCondition', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 100 }}"></TextArea>
				<core:Title text=""/>
				<Label text="OFP Description"/>
				<Text text="{path:'LocalDataModel>/OFPDescription'}"/>
				<Label text="Main Op Description"/>
				<Text text="{path:'LocalDataModel>/MainOpsCodeDescription'}"/>
				<Label text="Field Action Code"/>
				<Input required="{path:'DateModel>/oFieldActionInput'}" value="{path:'HeadSetData>/FieldActionReference'}" editable="{DateModel>/oFormEdit}"
					enabled="{DateModel>/oFieldActionInput}" change="onFieldActionInput"/>
				<Label text="Cause"/>
				<TextArea required="true" editable="{DateModel>/oFormEdit}"
					value="{path:'HeadSetData>/Cause',type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 100 }}"></TextArea>
				<core:Title text=""/>
				<Label text="Applied Coverage"/>
				<Text text="{HeadSetData>/AppliedWarrantyCoverage}"/>
				<Label text="T1 Code"/>
				<ComboBox showSecondaryValues="true" required="true" items="{ path: '/Zt1DescSrchhelpSet' }" selectedKey="{HeadSetData>/T1WarrantyCodes}"
					enabled="{DateModel>/oFormEdit}" value="{HeadSetData>/T1WarrantyCodes}">
					<core:ListItem key="{T1}" text="{path:'T1', constraints : { minLength : 2, maxLength : 100 }}" additionalText="{T1Text}"/>
				</ComboBox>
				<!--<Select selectedKey="{HeadSetData>/T1WarrantyCodes}" items="{ path: '/Zt1DescSrchhelpSet' }" enabled="{DateModel>/oFormEdit}">-->
				<!--	<core:Item key="{T1}" text="{T1} - {T1Text}"/>-->
				<!--</Select>-->
				<Label text="T2 Code"/>
				<ComboBox required="true" showSecondaryValues="true" items="{ path: '/Zt2DescSrchhelpSet' }" selectedKey="{HeadSetData>/T2WarrantyCodes}"
					enabled="{DateModel>/oFormEdit}" value="{HeadSetData>/T2WarrantyCodes}">
					<core:ListItem key="{path:'T2', constraints : { minLength : 2, maxLength : 100 }}" text="{T2}" additionalText="{T2Text}"/>
				</ComboBox>
				<!--<Select selectedKey="{HeadSetData>/T2WarrantyCodes}" items="{ path: '/Zt2DescSrchhelpSet' }" enabled="{DateModel>/oFormEdit}">-->
				<!--	<core:Item key="{T2}" text="{T2} - {T2Text}"/>-->
				<!--</Select>-->
				<Label text="Remedy"/>
				<TextArea required="true" editable="{DateModel>/oFormEdit}"
					value="{path:'HeadSetData>/Remedy', type : 'sap.ui.model.type.String', constraints : { minLength : 3, maxLength : 100 }}"></TextArea>
			</f:content>
		</f:SimpleForm>
		<Button text="View CCR Abbreviations" press="onPressAbbr"/>
		<f:SimpleForm visible="{DateModel>/AcA1}" editable="{DateModel>/oFormEdit}" title="Accessory Installation/Purchase"
			layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
			emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<Label text="Install Date"/>
				<DatePicker required="true" value="{path:'HeadSetData>/AccessoryInstallDate',type : 'sap.ui.model.type.Date'}"
					editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="Install Odometer"/>
				<Input value="{path:'HeadSetData>/AccessoryInstallOdometer'}" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm visible="{DateModel>/P1p2}" editable="{DateModel>/oFormEdit}" title="Original/Previous Installation"
			layout="ResponsiveGridLayout" labelSpanXL="12" labelSpanL="12" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0"
			emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="3" columnsM="3" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<Label text="Previous R.O./Invoice #"/>
				<Input value="{path:'HeadSetData>/PreviousROInvoice'}" editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="R.O./Invoice Date"/>
				<DatePicker required="true" value="{path:'HeadSetData>/PreviousROInvoiceDate',type : 'sap.ui.model.type.Date'}"
					editable="{DateModel>/oFormEdit}"/>
				<core:Title text=""/>
				<Label text="R.O. Odometer"/>
				<custom:CustomInput  value="{path:'HeadSetData>/PreviousROOdometer'}" editable="{DateModel>/oFormEdit}"/>
			</f:content>
		</f:SimpleForm>
		<f:SimpleForm editable="{DateModel>/oFormEdit}" title="Claim Comments" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3"
			labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="1" emptySpanL="1" emptySpanM="1" emptySpanS="0" columnsXL="1"
			columnsL="1" columnsM="1" singleContainerFullSize="false">
			<f:content>
				<core:Title text=""/>
				<TextArea height="8rem" value="{HeadSetData>/HeadText}" editable="{DateModel>/oFormEdit}">
					<layoutData>
						<l:GridData span="XL8 L8 M8 S12"/>
					</layoutData>
				</TextArea>
				<Button text="Add">
					<layoutData>
						<l:GridData span="XL2 L2 M3 S4"/>
					</layoutData>
				</Button>
			</f:content>
		</f:SimpleForm>
	</VBox>
	<VBox>
		<Panel width="auto" class="sapUiSmallMarginBottom" accessibleRole="Region">
			<headerToolbar>
				<Toolbar height="3rem">
					<Title text="Supporting Documents"/>
				</Toolbar>
			</headerToolbar>
			<content>
				<f:SimpleForm editable="{DateModel>/oFormEdit}" title="" layout="ResponsiveGridLayout" labelSpanXL="3" labelSpanL="3" labelSpanM="3"
					labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="0" emptySpanS="0" columnsXL="1" columnsL="1" columnsM="1"
					singleContainerFullSize="false">
					<f:content>
						<UploadCollection change="onUplaodChange" id="UploadSupportingDoc" uploadComplete="onUploadComplete" fileDeleted="onFileDeleted"
							beforeUploadStarts="onBeforeUpload" items="{ path: 'ClaimModel>/items', templateShareable: false }">
							<toolbar>
								<OverflowToolbar>
									<Breadcrumbs id="breadcrumbsSupportingDoc" currentLocationText="{ClaimModel>/currentLocationText}" links="{ClaimModel>/history}">
										<Link press="onBreadcrumbPress" text="{ClaimModel>fileName}"/>
									</Breadcrumbs>
									<ToolbarSpacer/>
									<UploadCollectionToolbarPlaceholder/>
								</OverflowToolbar>
							</toolbar>
							<items>
								<UploadCollectionItem selected="onSelectUpload"  documentId="{ClaimModel>DOC_ID}" fileName="{ClaimModel>FileName}" mimeType="{ClaimModel>MIMETYPE}"
									thumbnailUrl="{ClaimModel>url}" url="{ClaimModel>URI}" enableEdit="{settings>/enableEdit}" enableDelete="{settings>/enableDelete}"
									visibleDelete="{settings>/visibleDelete}" visibleEdit="{settings>/visibleEdit}"></UploadCollectionItem>
							</items>
						</UploadCollection>
					</f:content>
				</f:SimpleForm>
			</content>
		</Panel>
		<Panel width="auto" class="sapUiSmallMarginBottom" accessibleRole="Region">
			<content>
				<table:Table id="idRCPTable" class="classRCPTable" selectionMode="Single" rows="{path:'LocalDataModel>/ClaimSum'}"
					rowSelectionChange="onNavigate" selectionBehavior="Row" showNoData="true" threshold="4" visibleRowCount="4" enableBusyIndicator="true">
					<table:toolbar>
						<Toolbar>
							<Title id="idClaimWarrenty" text="Claim Amount Summary" level="H2"/>
							<ToolbarSpacer/>
							<!--<Button tooltip="View Settings" icon="sap-icon://drop-down-list" press="handleViewSettingsDialogButtonPressed"/>-->
						</Toolbar>
					</table:toolbar>
					<table:columns autoResizable="true">
						<table:Column width="17rem">
							<table:multiLabels>
								<Label text="(Totals included mark-up and discounts)" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="{path:'LocalDataModel>ItemType', formatter: 'zclaimProcessing.utils.formatter.fnFormatText'}" wrapping="true" textAlign="Center"
									width="100%"/>
							</table:template>
						</table:Column>
						<table:Column width="11rem">
							<table:multiLabels>
								<Label text="Dealer Claimed Amount" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="{LocalDataModel>DealerClaim}" wrapping="true" textAlign="Center" width="100%"/>
							</table:template>
						</table:Column>
						<table:Column width="11rem">
							<table:multiLabels>
								<Label text="TCI Approved Amount" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="{LocalDataModel>TCIAppr}" wrapping="true" textAlign="Center" width="100%"/>
							</table:template>
						</table:Column>
						<table:Column width="11rem">
							<table:multiLabels>
								<Label text="Difference (CAD)" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="{LocalDataModel>Diff}" wrapping="true" textAlign="Center" width="100%"/>
							</table:template>
						</table:Column>
						<table:Column width="8rem" headerSpan="{multiHeaderConfig>/multiheader2}">
							<table:multiLabels>
								<Label text="GST/HST" textAlign="Center" width="100%"/>
								<Label text="Amount" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="{LocalDataModel>GSTHSTAmt}" wrapping="true" textAlign="Center" width="100%"/>
							</table:template>
						</table:Column>
						<table:Column width="8rem">
							<table:multiLabels>
								<Label text="GST/HST" textAlign="Center" width="100%"/>
								<Label text="Tax %" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="{LocalDataModel>GSTHSTRate}" wrapping="true" textAlign="Center" width="100%"/>
							</table:template>
						</table:Column>
						<table:Column width="8rem" headerSpan="{multiHeaderConfig>/multiheader2}">
							<table:multiLabels>
								<Label text="PST/QST" textAlign="Center" width="100%"/>
								<Label text="Amount" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="{LocalDataModel>PSTQSTAmt}" wrapping="true" textAlign="Center" width="100%"/>
							</table:template>
						</table:Column>
						<table:Column width="8rem">
							<table:multiLabels>
								<Label text="PST/QST" textAlign="Center" width="100%"/>
								<Label text="Tax %" textAlign="Center" width="100%"/>
							</table:multiLabels>
							<table:template>
								<Text text="{LocalDataModel>PSTQSTRate}" wrapping="true" textAlign="Center" width="100%"/>
							</table:template>
						</table:Column>
						<table:Column width="17rem">
							<table:multiLabels>
								<Label text="TCI Approved Amount Including Tax" textAlign="Center" width="100%"/>
