		<Button text="{i18n>NewClaim}" press="handleNavHeaderPress" type="Transparent" visible="{HeaderLinksModel>/NewClaim}"></Button>
		<Button text="{i18n>ViewUpdateClaims}" press="handleNavHeaderPress" type="Transparent" visible="{HeaderLinksModel>/ViewUpdateClaims}"></Button>
		<Button text="{i18n>QuickCoverageTool}" press="handleNavHeaderPress" type="Transparent" visible="{HeaderLinksModel>/QuickCoverageTool}"></Button>
		<Button text="{i18n>ClaimInquiry}" press="handleNavHeaderPress" type="Transparent" visible="{HeaderLinksModel>/ClaimInquiry}"></Button>
