<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:u="sap.ui.unified" xmlns:l="sap.ui.layout"
	xmlns="sap.m" xmlns:table="sap.ui.table">
	<Dialog class="sapUiSizeCompact" title="{i18n>SpecialHandlingDetailsVIN}">
		<content>
			<table:Table class="classRCPTable" selectionMode="MultiToggle" rows="{LocalDataModel>/SPWROF}" visibleRowCountMode="Auto"
				rowSelectionChange="onNavigatePaint" selectionBehavior="Row" showNoData="true" threshold="20" enableBusyIndicator="true">
				<table:columns autoResizable="true">
					<table:Column width="17rem">
						<table:multiLabels>
							<Label text="{i18n>Code}" textAlign="Center" width="100%"/>
						</table:multiLabels>
						<table:template>
							<Text text="{LocalDataModel>SPHLCode}" wrapping="false"/>
						</table:template>
					</table:Column>
					<table:Column width="17rem">
						<table:multiLabels>
							<Label text="{i18n>Description}" textAlign="Center" width="100%"/>
						</table:multiLabels>
						<table:template>
							<Text text="{LocalDataModel>SphlCodeDesc}" wrapping="false"/>
						</table:template>
					</table:Column>
					<table:Column width="17rem">
						<table:multiLabels>
							<Label text="{i18n>DateIssued}" textAlign="Center" width="100%"/>
						</table:multiLabels>
						<table:template>
							<Text text="{LocalDataModel>SphlIssuedDate}" wrapping="false"/>
						</table:template>
					</table:Column>
				</table:columns>
			</table:Table>
		</content>
		<HBox class="sapUiSmallMarginTop">
			<Button class="sapUiSmallMarginBeginEnd" type="Emphasized" text="Close" press="onCloseWrittenOf"/>
		</HBox>
	</Dialog>
</core:FragmentDefinition>