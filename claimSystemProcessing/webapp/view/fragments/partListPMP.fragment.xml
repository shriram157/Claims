<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog noDataText="{i18n>NoData}" title="{i18n>SelectPartNumber}" search="_handleValueHelpSearch" liveChange="_handleLiveSearch"
		confirm="_handleValueHelpClose" cancel="_handleValueHelpClose" 
		items="{path:'ProductMasterModel>/C_Product', filters: [ { filters: [ { path: 'ProductType', operator: 'NE', value1: 'VERP' }, { path: 'ProductType', operator: 'NE', value1: 'DIEN' }, { path: 'ProductType', operator: 'NE', value1: 'VEHI' } ], and: true } ]}">
		<ColumnListItem>
			<cells>
				<Text text="{ProductMasterModel>Product}"/>
				<Text text="{ProductMasterModel>ProductDescription}"/>
				<Text text="{ProductMasterModel>BaseUnit}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="{i18n>Material}"/>
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text text="Description"/>
				</header>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true" hAlign="End">
				<header>
					<Text text="{i18n>Unit}"/>
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>

