			<VBox id="idPartArea" visible="{multiHeaderConfig>/partDamage}" class="clBgColor sapUiSmallMarginTop" wrap="Wrap" width="100%">
				<f:SimpleForm editable="true" labelSpanL="4" width="90%" title="" labelSpanXL="12" layout="ResponsiveGridLayout" emptySpanXL="1" emptySpanL="1"
					columnsM="2" columnsL="1" columnsXL="1">
					<f:content>
						<Label text="Discrepancy Type" textAlign="Begin"/>
						<Select width="16rem">
							<items>
								<core:Item text="Part shipped in cage" key=""/>
								<core:Item text="Packaging damage" key=""/>
								<core:Item text="No outside packaging" key=""/>
								<core:Item text="" key="No damage to outside packaging"/>
								<core:Item text="" key="Windshield"/>
							</items>
						</Select>
						<Button text="Save Part" class="sapUiSmallMarginEnd">
							<layoutData>
								<l:GridData span="XL1 L1 M3 S4"/>
							</layoutData>
						</Button>
					</f:content>
				</f:SimpleForm>
				<f:SimpleForm id="idRow01Form" class="sapUiSmallMarginBegin" editable="true" labelSpanL="12" title="" labelSpanXL="12" layout="ResponsiveGridLayout"
					columnsM="2" columnsL="5" columnsXL="5">
					<f:content>
						<core:Title text="Ordered"/>
						<Label text="Line #:"></Label>
						<Input value="{ 
                                 path : 'HeadSetData>/text', 
                                  type : 'sap.ui.model.type.String', 
                                  constraints : { 
                                      minLength : 3, 
                                      maxLength : 10 
                                  }
                              }" required="true"></Input>
						<core:Title text=""/>
						<Label text="Part Description"></Label>
						<Input id="idOrderedPartDesc" editable="{multiHeaderConfig>/OrderedPartDesc}"></Input>
						<core:Title text=""/>
						<Label text="Dealer Net Price"></Label>
						<Input editable="{multiHeaderConfig>/DealerNetPrcEdt}"></Input>
						<core:Title text=""/>
						<Label text="Quantity"></Label>
						<Input value="{ 
                                 path : 'HeadSetData>/text', 
                                  type : 'sap.ui.model.type.String', 
                                  constraints : { 
                                      minLength : 3, 
                                      maxLength : 10 
                                  }
                              }" required="true">
							<layoutData>
								<l:GridData span="XL8 L8"/>
							</layoutData>
						</Input>
						<core:Title text=""/>
						<Label text="Part Number Ordered"></Label>
						<Input value="{ 
                                 path : 'HeadSetData>/text', 
                                  type : 'sap.ui.model.type.String', 
                                  constraints : { 
                                      minLength : 3, 
                                      maxLength : 10 
                                  }
                              }" required="true"></Input>
					</f:content>
				</f:SimpleForm>
				<f:SimpleForm id="idRow02Form" class="sapUiSmallMarginBegin sapUiSmallMarginBottom" editable="true" title="" layout="ResponsiveGridLayout" labelSpanL="12"
					labelSpanXL="12" columnsM="2" columnsL="{multiHeaderConfig>/multiheader5}" columnsXL="{multiHeaderConfig>/multiheader5}">
					<f:content>
						<core:Title text="Claimed"/>
						<UploadCollection id="UploadCollection" change="onUplaodChange" uploadComplete="onUploadComplete02" fileDeleted="onFileDeleted02"
							visible="{multiHeaderConfig>/uploader}">
							<toolbar>
								<OverflowToolbar>
									<Breadcrumbs id="breadcrumbs" currentLocationText="{AttachmentModel>/currentLocationText}" links="{AttachmentModel>/history}">
										<Link press="onBreadcrumbPress" text="{AttachmentModel>name}"/>
									</Breadcrumbs>
									<ToolbarSpacer/>
									<UploadCollectionToolbarPlaceholder/>
								</OverflowToolbar>
							</toolbar>
						</UploadCollection>
						<Label text="Retain Part" visible="{multiHeaderConfig>/RetainPartV}"></Label>
						<Input value="{ 
                                 path : 'HeadSetData>/text', 
                                  type : 'sap.ui.model.type.String', 
                                  constraints : { 
                                      minLength : 3, 
                                      maxLength : 10 
                                  }
                              }" required="true" visible="{multiHeaderConfig>/RetainPartV}"></Input>
						<core:Title text=""/>
						<Label text="Part Description" visible="{multiHeaderConfig>/PartDescriptionOrdRcv}"></Label>
						<Input id="idOrderedReceivedPartDesc" editable="{multiHeaderConfig>/OrderedPartDesc}" visible="{multiHeaderConfig>/PartDescriptionOrdRcv}"></Input>
						<Label text="Part Repaired?" visible="{multiHeaderConfig>/PartRepaired}"></Label>
						<Select visible="{multiHeaderConfig>/PartRepaired}">
							<items>
								<core:Item text="Yes"/>
								<core:Item text="No"/>
							</items>
						</Select>
						<core:Title text=""/>
						<Label text="Dealer Net Price" visible="{multiHeaderConfig>/DealerNetPrcV}"></Label>
						<Input id="idDealerNetPrc" visible="{multiHeaderConfig>/DealerNetPrcV}" editable="{multiHeaderConfig>/DealerNetPrcEdt}"></Input>
						<Label text="Repair Amount" visible="{multiHeaderConfig>/RepairAmtV}"></Label>
						<Input id="idReceivedAmt" value="" type="Number" visible="{multiHeaderConfig>/RepairAmtV}"></Input>
						<core:Title text=""/>
						<Label text="Quantity"></Label>
						<Input value="{ 
                                  path : 'HeadSetData>/number', 
                                  type : 'sap.ui.model.type.Integer', 
                                  formatOptions : {
                                      groupingEnabled : true
                                  },
                                  constraints : { 
                                      minimum : 50, 
                                      maximum : 10000000 
                                  }
                              }" required="true" id="idReceivedQnt">
							<layoutData>
								<l:GridData span="XL8 L8"/>
							</layoutData>
						</Input>
						<core:Title text=""/>
						<Label text="Part Number Received" visible="{multiHeaderConfig>/PartNumberRcV}"></Label>
						<Input value="{ 
                                 path : 'HeadSetData>/text', 
                                  type : 'sap.ui.model.type.String', 
                                  constraints : { 
                                      minLength : 3, 
                                      maxLength : 10 
                                  }
                              }" required="true" visible="{multiHeaderConfig>/PartNumberRcV}"></Input>
					</f:content>
				</f:SimpleForm>
