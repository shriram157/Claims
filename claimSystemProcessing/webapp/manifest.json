{
	"_version": "1.8.0",
	"sap.app": {
		"id": "zclaimProcessing",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "2.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZDLR_CLAIM_SRV": {
				"uri": "/node/ZDLR_CLAIM_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZDLR_CLAIM_SRV/metadata.xml"
				}
			},
			"ZDLR_CLAIM_PMP_SRV": {
				"uri": "/node/ZDLR_CLAIM_PMP_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZDLR_CLAIM_PMP_SRV/metadata.xml"
				}
			},

			"API_BUSINESS_PARTNER": {
				"uri": "/node/API_BUSINESS_PARTNER/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/API_BUSINESS_PARTNER/metadata.xml"
				}
			},
			"MD_PRODUCT_OP_SRV": {
				"uri": "/node/MD_PRODUCT_OP_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/MD_PRODUCT_OP_SRV/metadata.xml"
				}
			},
			"Z_VEHICLE_MASTER_SRV": {
				"uri": "/node/Z_VEHICLE_MASTER_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/Z_VEHICLE_MASTER_SRV/metadata.xml"
				}
			},
			"ZECP_SALES_ODATA_SERVICE_SRV": {
				"uri": "/node/ZECP_SALES_ODATA_SERVICE_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZECP_SALES_ODATA_SERVICE_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "zclaimProcessing.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "zclaimProcessing.i18n.i18n"
				}
			},
			"ProssingModel": {

				"type": "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "ZDLR_CLAIM_SRV",
				"preload": true,
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"useBatch": false,
					"defaultUpdateMethod": "PUT",
					"headers": {
						"X-Requested-With": "XMLHttpRequest"
					}
				}
			},
			"ApiBusinessModel": {

				"type": "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "API_BUSINESS_PARTNER",
				"preload": true,
				"settings": {
					"defaultOperationMode": "Server",
					"defaultCountMode": "Request",
					"defaultBindingMode": "TwoWay",
					"defaultUpdateMethod": "PATCH",
					"useBatch": true
				}
			},
			"ZVehicleMasterModel": {

				"type": "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "Z_VEHICLE_MASTER_SRV",
				"preload": true,
				"settings": {
					"defaultOperationMode": "Server",
					"defaultCountMode": "Request",
					"defaultBindingMode": "TwoWay",
					"defaultUpdateMethod": "PATCH",
					"useBatch": false
				}
			},
			"zDLRCLAIMPMPSRV": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "ZDLR_CLAIM_PMP_SRV",
				"preload": true,
				"settings": {
					"defaultOperationMode": "Server",
					"defaultCountMode": "Request",
					"defaultBindingMode": "TwoWay",
					"useBatch": false
					
				}
			},
			"EcpSalesModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "ZECP_SALES_ODATA_SERVICE_SRV",
				"preload": true,
				"settings": {
					"defaultOperationMode": "Server",
					"defaultCountMode": "Request",
					"defaultBindingMode": "TwoWay",
					"useBatch": false
				}
			},
			"ProductMaster": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "MD_PRODUCT_OP_SRV",
				"preload": true,
				"settings": {
					"defaultOperationMode": "Server",
					"defaultCountMode": "Request",
					"defaultBindingMode": "TwoWay",
					"defaultUpdateMethod": "PATCH",
					"useBatch": true
				}
			}
		},
		"resources": {
			"js": [

				{
					"uri": "libs/moment.js"
				}, {
					"uri": "utils/formatter.js"
				}, {
					"uri": "utils/changeFabicon.js"
				}
			],
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "zclaimProcessing.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "SearchClaim",
				"pattern": "Search",
				"target": [
					"SearchClaim"
				]
			}, {
				"name": "NewClaimSelectGroup",
				"pattern": "NewClaim",
				"target": [
					"NewClaimSelectGroup"
				]
			}, {
				"name": "MainClaimSection",
				"pattern": "MainClaim/{claimNum}/{oKey}/{oClaimGroup}/{oClaimNav}/{claimTypeGroup}",
				"target": [
					"MainClaimSection"
				]
			}, {
				"name": "PMPMainSection",
				"pattern": "PMPMain/{claimNum}/{oKey}/{oClaimGroup}/{oClaimNav}/{claimTypeGroup}",
				"target": [
					"PMPMainSection"
				]
			}, {
				"name": "PartsMainSection",
				"pattern": "PartsMain/{claimNum}/{oKey}/{oClaimGroup}/{oClaimNav}/{claimTypeGroup}",
				"target": [
					"PartsMainSection"
				]
			}, {
				"name": "QueryCoverageTools",
				"pattern": "QueryCoverage",
				"target": [
					"QueryCoverageTools"
				]
			}, {
				"name": "ClaimInquiry",
				"pattern": "Inquiry",
				"target": [
					"ClaimInquiry"
				]
			}],
			"targets": {
				"SearchClaim": {
					"viewType": "XML",
					"viewName": "SearchClaim",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "zclaimProcessing.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"MainClaimSection": {
					"viewType": "XML",
					"viewName": "MainClaimSection",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "zclaimProcessing.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"NewClaimSelectGroup": {
					"viewType": "XML",
					"viewName": "NewClaimSelectGroup",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "zclaimProcessing.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},

				"PartsMainSection": {
					"viewType": "XML",
					"viewName": "PartsMainSection"
				},
				"QueryCoverageTools": {
					"viewType": "XML",
					"viewName": "QueryCoverageTools"
				},
				"ClaimInquiry": {
					"viewType": "XML",
					"viewName": "ClaimInquiry"
				},
				"ClaimInquiryDetails": {
					"viewType": "XML",
					"viewName": "ClaimInquiryDetails"
				},
				"PMPMainSection": {
					"viewType": "XML",
					"viewName": "PMPMainSection"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zclaimsystem/webapp",
		"_version": "1.1.0"
	}
}