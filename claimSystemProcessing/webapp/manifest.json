{
	"_version": "1.8.0",
	"sap.app": {
		"id": "zclaimProcessing",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"ZDLR_CLAIM_SRV": {
				"uri": "/node/ZDLR_CLAIM_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZDLR_CLAIM_SRV/metadata.xml"
				}
			},
			"API_BUSINESS_PARTNER": {
				"uri": "/node/API_BUSINESS_PARTNER/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/API_BUSINESS_PARTNER/metadata.xml"
				}
			},
			"MD_PRODUCT_FS_SRV": {
				"uri": "/node/MD_PRODUCT_FS_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/MD_PRODUCT_FS_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "zclaimProcessing.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "zclaimProcessing.i18n.i18n"
				}
			},
			"": {
				"uri": "/node/ZDLR_CLAIM_SRV/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "ZDLR_CLAIM_SRV",
				"preload": false
			},
			"API_BUSINESS_PARTNER": {
				"uri": "/node/API_BUSINESS_PARTNER/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "API_BUSINESS_PARTNER",
				"preload": false
			}
		},
		"resources": {
			"js": [
				{
					"uri": "utils/formatter.js"
				}
			],
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "zclaimProcessing.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"name": "SearchClaim",
					"pattern": "",
					"target": [
						"SearchClaim"
					]
				},
				{
					"name": "NewClaimSelectGroup",
					"pattern": "NewClaim",
					"target": [
						"NewClaimSelectGroup"
					]
				},
				{
					"name": "MainClaimSection",
					"pattern": "MainClaim/{claimNum}",
					"target": [
						"MainClaimSection"
					]
				},
				{
					"name": "PartsMainSection",
					"pattern": "PartsMain/{claimNum}",
					"target": [
						"PartsMainSection"
					]
				},
				{
					"name": "QueryCoverageTools",
					"pattern": "QueryCoverage",
					"target": [
						"QueryCoverageTools"
					]
				},
				{
					"name": "ClaimInquiry",
					"pattern": "Inquiry",
					"target": [
						"ClaimInquiry"
					]
				}
			],
			"targets": {
				"SearchClaim": {
					"viewType": "XML",
					"viewName": "SearchClaim",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "zclaimProcessing.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"MainClaimSection": {
					"viewType": "XML",
					"viewName": "MainClaimSection",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "zclaimProcessing.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"NewClaimSelectGroup": {
					"viewType": "XML",
					"viewName": "NewClaimSelectGroup",
					"routerClass": "sap.m.routing.Router",
					"async": true,
					"viewPath": "zclaimProcessing.view",
					"controlAggregation": "pages",
					"controlId": "idAppControl",
					"clearControlAggregation": false
				},
				"PartsMainSection": {
					"viewType": "XML",
					"viewName": "PartsMainSection"
				},
				"QueryCoverageTools": {
					"viewType": "XML",
					"viewName": "QueryCoverageTools"
				},
				"ClaimInquiry": {
					"viewType": "XML",
					"viewName": "ClaimInquiry"
				},
				"ClaimInquiryDetails": {
					"viewType": "XML",
					"viewName": "ClaimInquiryDetails"
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zclaimsystem/webapp",
		"_version": "1.1.0"
	}
}